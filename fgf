[32m+[m[32m/**[m
[32m+[m[32m * Flash (http://jquery.lukelutman.com/plugins/flash)[m
[32m+[m[32m * A jQuery plugin for embedding Flash movies.[m
[32m+[m[32m *[m[41m [m
[32m+[m[32m * Version 1.0[m
[32m+[m[32m * November 9th, 2006[m
[32m+[m[32m *[m
[32m+[m[32m * Copyright (c) 2006 Luke Lutman (http://www.lukelutman.com)[m
[32m+[m[32m * Dual licensed under the MIT and GPL licenses.[m
[32m+[m[32m * http://www.opensource.org/licenses/mit-license.php[m
[32m+[m[32m * http://www.opensource.org/licenses/gpl-license.php[m
[32m+[m[32m *[m[41m [m
[32m+[m[32m * Inspired by:[m
[32m+[m[32m * SWFObject (http://blog.deconcept.com/swfobject/)[m
[32m+[m[32m * UFO (http://www.bobbyvandersluis.com/ufo/)[m
[32m+[m[32m * sIFR (http://www.mikeindustries.com/sifr/)[m
[32m+[m[32m *[m[41m [m
[32m+[m[32m * IMPORTANT:[m[41m [m
[32m+[m[32m * The packed version of jQuery breaks ActiveX control[m
[32m+[m[32m * activation in Internet Explorer. Use JSMin to minifiy[m
[32m+[m[32m * jQuery (see: http://jquery.lukelutman.com/plugins/flash#activex).[m
[32m+[m[32m *[m
[32m+[m[32m **/[m[41m [m
[32m+[m[32m;(function(){[m
[32m+[m[41m	[m
[32m+[m[32mvar $$;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m *[m[41m [m
[32m+[m[32m * @desc Replace matching elements with a flash movie.[m
[32m+[m[32m * @author Luke Lutman[m
[32m+[m[32m * @version 1.0.1[m
[32m+[m[32m *[m
[32m+[m[32m * @name flash[m
[32m+[m[32m * @param Hash htmlOptions Options for the embed/object tag.[m
[32m+[m[32m * @param Hash pluginOptions Options for detecting/updating the Flash plugin (optional).[m
[32m+[m[32m * @param Function replace Custom block called for each matched element if flash is installed (optional).[m
[32m+[m[32m * @param Function update Custom block called for each matched if flash isn't installed (optional).[m
[32m+[m[32m * @type jQuery[m
[32m+[m[32m *[m
[32m+[m[32m * @cat plugins/flash[m
[32m+[m[32m *[m[41m [m
[32m+[m[32m * @example $('#hello').flash({ src: 'hello.swf' });[m
[32m+[m[32m * @desc Embed a Flash movie.[m
[32m+[m[32m *[m
[32m+[m[32m * @example $('#hello').flash({ src: 'hello.swf' }, { version: 8 });[m
[32m+[m[32m * @desc Embed a Flash 8 movie.[m
[32m+[m[32m *[m
[32m+[m[32m * @example $('#hello').flash({ src: 'hello.swf' }, { expressInstall: true });[m
[32m+[m[32m * @desc Embed a Flash movie using Express Install if flash isn't installed.[m
[32m+[m[32m *[m
[32m+[m[32m * @example $('#hello').flash({ src: 'hello.swf' }, { update: false });[m
[32m+[m[32m * @desc Embed a Flash movie, don't show an update message if Flash isn't installed.[m
[32m+[m[32m *[m
[32m+[m[32m**/[m
[32m+[m[32m$$ = jQuery.fn.flash = function(htmlOptions, pluginOptions, replace, update) {[m
[32m+[m[41m	[m
[32m+[m	[32m// Set the default block.[m
[32m+[m	[32mvar block = replace || $$.replace;[m
[32m+[m[41m	[m
[32m+[m	[32m// Merge the default and passed plugin options.[m
[32m+[m	[32mpluginOptions = $$.copy($$.pluginOptions, pluginOptions);[m
[32m+[m[41m	[m
[32m+[m	[32m// Detect Flash.[m
[32m+[m	[32mif(!$$.hasFlash(pluginOptions.version)) {[m
[32m+[m		[32m// Use Express Install (if specified and Flash plugin 6,0,65 or higher is installed).[m
[32m+[m		[32mif(pluginOptions.expressInstall && $$.hasFlash(6,0,65)) {[m
[32m+[m			[32m// Add the necessary flashvars (merged later).[m
[32m+[m			[32mvar expressInstallOptions = {[m
[32m+[m				[32mflashvars: {[m[41m  	[m
[32m+[m					[32mMMredirectURL: location,[m
[32m+[m					[32mMMplayerType: 'PlugIn',[m
[32m+[m					[32mMMdoctitle: jQuery('title').text()[m[41m [m
[32m+[m				[32m}[m[41m					[m
[32m+[m			[32m};[m
[32m+[m		[32m// Ask the user to update (if specified).[m
[32m+[m		[32m} else if (pluginOptions.update) {[m
[32m+[m			[32m// Change the block to insert the update message instead of the flash movie.[m
[32m+[m			[32mblock = update || $$.update;[m
[32m+[m		[32m// Fail[m
[32m+[m		[32m} else {[m
[32m+[m			[32m// The required version of flash isn't installed.[m
[32m+[m			[32m// Express Install is turned off, or flash 6,0,65 isn't installed.[m
[32m+[m			[32m// Update is turned off.[m
[32m+[m			[32m// Return without doing anything.[m
[32m+[m			[32mreturn this;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[41m	[m
[32m+[m	[32m// Merge the default, express install and passed html options.[m
[32m+[m	[32mhtmlOptions = $$.copy($$.htmlOptions, expressInstallOptions, htmlOptions);[m
[32m+[m[41m	[m
[32m+[m	[32m// Invoke $block (with a copy of the merged html options) for each element.[m
[32m+[m	[32mreturn this.each(function(){[m
[32m+[m		[32mblock.call(this, $$.copy(htmlOptions));[m
[32m+[m	[32m});[m
[32m+[m[41m	[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m *[m
[32m+[m[32m * @name flash.copy[m
[32m+[m[32m * @desc Copy an arbitrary number of objects into a new object.[m
[32m+[m[32m * @type Object[m
[32m+[m[32m *[m[41m [m
[32m+[m[32m * @example $$.copy({ foo: 1 }, { bar: 2 });[m
[32m+[m[32m * @result { foo: 1, bar: 2 };[m
[32m+[m[32m *[m
[32m+[m[32m**/[m
[32m+[m[32m$$.copy = function() {[m
[32m+[m	[32mvar options = {}, flashvars = {};[m
[32m+[m	[32mfor(var i = 0; i < arguments.length; i++) {[m
[32m+[m		[32mvar arg = arguments[i];[m
[32m+[m		[32mif(arg == undefined) continue;[m
[32m+[m		[32mjQuery.extend(options, arg);[m
[32m+[m		[32m// don't clobber one flash vars object with another[m
[32m+[m		[32m// merge them instead[m
[32m+[m		[32mif(arg.flashvars == undefined) continue;[m
[32m+[m		[32mjQuery.extend(flashvars, arg.flashvars);[m
[32m+[m	[32m}[m
[32m+[m	[32moptions.flashvars = flashvars;[m
[32m+[m	[32mreturn options;[m
[32m+[m[32m};[m
[32m+[m[32m/*[m
[32m+[m[32m * @name flash.hasFlash[m
[32m+[m[32m * @desc Check if a specific version of the Flash plugin is installed[m
[32m+[m[32m * @type Boolean[m
[32m+[m[32m *[m
[32m+[m[32m**/[m
[32m+[m[32m$$.hasFlash = function() {[m
[32m+[m	[32m// look for a flag in the query string to bypass flash detection[m
[32m+[m	[32mif(/hasFlash\=true/.test(location)) return true;[m
[32m+[m	[32mif(/hasFlash\=false/.test(location)) return false;[m
[32m+[m	[32mvar pv = $$.hasFlash.playerVersion().match(/\d+/g);[m
[32m+[m	[32mvar rv = String([arguments[0], arguments[1], arguments[2]]).match(/\d+/g) || String($$.pluginOptions.version).match(/\d+/g);[m
[32m+[m	[32mfor(var i = 0; i < 3; i++) {[m
[32m+[m		[32mpv[i] = parseInt(pv[i] || 0);[m
[32m+[m		[32mrv[i] = parseInt(rv[i] || 0);[m
[32m+[m		[32m// player is less than required[m
[32m+[m		[32mif(pv[i] < rv[i]) return false;[m
[32m+[m		[32m// player is greater than required[m
[32m+[m		[32mif(pv[i] > rv[i]) return true;[m
[32m+[m	[32m}[m
[32m+[m	[32m// major version, minor version and revision match exactly[m
[32m+[m	[32mreturn true;[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m *[m
[32m+[m[32m * @name flash.hasFlash.playerVersion[m
[32m+[m[32m * @desc Get the version of the installed Flash plugin.[m
[32m+[m[32m * @type String[m
[32m+[m[32m *[m
[32m+[m[32m**/[m
[32m+[m[32m$$.hasFlash.playerVersion = function() {[m
[32m+[m	[32m// ie[m
[32m+[m	[32mtry {[m
[32m+[m		[32mtry {[m
[32m+[m			[32m// avoid fp6 minor version lookup issues[m
[32m+[m			[32m// see: http://blog.deconcept.com/2006/01/11/getvariable-setvariable-crash-internet-explorer-flash-6/[m
[32m+[m			[32mvar axo = new ActiveXObject('ShockwaveFlash.ShockwaveFlash.6');[m
[32m+[m			[32mtry { axo.AllowScriptAccess = 'always';	}[m[41m [m
[32m+[m			[32mcatch(e) { return '6,0,0'; }[m[41m				[m
[32m+[m		[32m} catch(e) {}[m
[32m+[m		[32mreturn new ActiveXObject('ShockwaveFlash.ShockwaveFlash').GetVariable('$version').replace(/\D+/g, ',').match(/^,?(.+),?$/)[1];[m
[32m+[m	[32m// other browsers[m
[32m+[m	[32m} catch(e) {[m
[32m+[m		[32mtry {[m
[32m+[m			[32mif(navigator.mimeTypes["application/x-shockwave-flash"].enabledPlugin){[m
[32m+[m				[32mreturn (navigator.plugins["Shockwave Flash 2.0"] || navigator.plugins["Shockwave Flash"]).description.replace(/\D+/g, ",").match(/^,?(.+),?$/)[1];[m
[32m+[m			[32m}[m
[32m+[m		[32m} catch(e) {}[m[41m		[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn '0,0,0';[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m *[m
[32m+[m[32m * @name flash.htmlOptions[m
[32m+[m[32m * @desc The default set of options for the object or embed tag.[m
[32m+[m[32m *[m
[32m+[m[32m**/[m
[32m+[m[32m$$.htmlOptions = {[m
[32m+[m	[32mheight: 240,[m
[32m+[m	[32mflashvars: {},[m
[32m+[m	[32mpluginspage: 'http://www.adobe.com/go/getflashplayer',[m
[32m+[m	[32msrc: '#',[m
[32m+[m	[32mtype: 'application/x-shockwave-flash',[m
[32m+[m	[32mwidth: 320[m[41m		[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m *[m
[32m+[m[32m * @name flash.pluginOptions[m
[32m+[m[32m * @desc The default set of options for checking/updating the flash Plugin.[m
[32m+[m[32m *[m
[32m+[m[32m**/[m
[32m+[m[32m$$.pluginOptions = {[m
[32m+[m	[32mexpressInstall: false,[m
[32m+[m	[32mupdate: true,[m
[32m+[m	[32mversion: '6.0.65'[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m *[m
[32m+[m[32m * @name flash.replace[m
[32m+[m[32m * @desc The default method for replacing an element with a Flash movie.[m
[32m+[m[32m *[m
[32m+[m[32m**/[m
[32m+[m[32m$$.replace = function(htmlOptions) {[m
[32m+[m	[32mthis.innerHTML = '<div class="alt">'+this.innerHTML+'</div>';[m
[32m+[m	[32mjQuery(this)[m
[32m+[m		[32m.addClass('flash-replaced')[m
[32m+[m		[32m.prepend($$.transform(htmlOptions));[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m *[m
[32m+[m[32m * @name flash.update[m
[32m+[m[32m * @desc The default method for replacing an element with an update message.[m
[32m+[m[32m *[m
[32m+[m[32m**/[m
[32m+[m[32m$$.update = function(htmlOptions) {[m
[32m+[m	[32mvar url = String(location).split('?');[m
[32m+[m	[32murl.splice(1,0,'?hasFlash=true&');[m
[32m+[m	[32murl = url.join('');[m
[32m+[m	[32mvar msg = '<p>This content requires the Flash Player. <a href="http://www.adobe.com/go/getflashplayer">Download Flash Player</a>. Already have Flash Player? <a href="'+url+'">Click here.</a></p>';[m
[32m+[m	[32mthis.innerHTML = '<span class="alt">'+this.innerHTML+'</span>';[m
[32m+[m	[32mjQuery(this)[m
[32m+[m		[32m.addClass('flash-update')[m
[32m+[m		[32m.prepend(msg);[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m *[m
[32m+[m[32m * @desc Convert a hash of html options to a string of attributes, using Function.apply().[m[41m [m
[32m+[m[32m * @example toAttributeString.apply(htmlOptions)[m
[32m+[m[32m * @result foo="bar" foo="bar"[m
[32m+[m[32m *[m
[32m+[m[32m**/[m
[32m+[m[32mfunction toAttributeString() {[m
[32m+[m	[32mvar s = '';[m
[32m+[m	[32mfor(var key in this)[m
[32m+[m		[32mif(typeof this[key] != 'function')[m
[32m+[m			[32ms += key+'="'+this[key]+'" ';[m
[32m+[m	[32mreturn s;[m[41m		[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m *[m
[32m+[m[32m * @desc Convert a hash of flashvars to a url-encoded string, using Function.apply().[m[41m [m
[32m+[m[32m * @example toFlashvarsString.apply(flashvarsObject)[m
[32m+[m[32m * @result foo=bar&foo=bar[m
[32m+[m[32m *[m
[32m+[m[32m**/[m
[32m+[m[32mfunction toFlashvarsString() {[m
[32m+[m	[32mvar s = '';[m
[32m+[m	[32mfor(var key in this)[m
[32m+[m		[32mif(typeof this[key] != 'function')[m
[32m+[m			[32ms += key+'='+encodeURIComponent(this[key])+'&';[m
[32m+[m	[32mreturn s.replace(/&$/, '');[m[41m		[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m *[m
[32m+[m[32m * @name flash.transform[m
[32m+[m[32m * @desc Transform a set of html options into an embed tag.[m
[32m+[m[32m * @type String[m[41m [m
[32m+[m[32m *[m
[32m+[m[32m * @example $$.transform(htmlOptions)[m
[32m+[m[32m * @result <embed src="foo.swf" ... />[m
[32m+[m[32m *[m
[32m+[m[32m * Note: The embed tag is NOT standards-compliant, but it[m[41m [m
[32m+[m[32m * works in all current browsers. flash.transform can be[m
[32m+[m[32m * overwritten with a custom function to generate more[m[41m [m
[32m+[m[32m * standards-compliant markup.[m
[32m+[m[32m *[m
[32m+[m[32m**/[m
[32m+[m[32m$$.transform = function(htmlOptions) {[m
[32m+[m	[32mhtmlOptions.toString = toAttributeString;[m
[32m+[m	[32mif(htmlOptions.flashvars) htmlOptions.flashvars.toString = toFlashvarsString;[m
[32m+[m	[32mreturn '<embed ' + String(htmlOptions) + '/>';[m[41m		[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m *[m
[32m+[m[32m * Flash Player 9 Fix (http://blog.deconcept.com/2006/07/28/swfobject-143-released/)[m
[32m+[m[32m *[m
[32m+[m[32m**/[m
[32m+[m[32mif (window.attachEvent) {[m
[32m+[m	[32mwindow.attachEvent("onbeforeunload", function(){[m
[32m+[m		[32m__flash_unloadHandler = function() {};[m
[32m+[m		[32m__flash_savedUnloadHandler = function() {};[m
[32m+[m	[32m});[m
[32m+[m[32m}[m
[32m+[m[41m	[m
[32m+[m[32m})();[m
\ No newline at end of file[m
[1mdiff --git a/gitnn/Advertising/jquery.inview.js b/gitnn/Advertising/jquery.inview.js[m
[1mnew file mode 100644[m
[1mindex 0000000..5ccd098[m
[1m--- /dev/null[m
[1m+++ b/gitnn/Advertising/jquery.inview.js[m
[36m@@ -0,0 +1,60 @@[m
[32m+[m[32m/**[m
[32m+[m[32m * author Remy Sharp[m
[32m+[m[32m * url http://remysharp.com/2009/01/26/element-in-view-event-plugin/[m
[32m+[m[32m */[m
[32m+[m[32m(function ($) {[m
[32m+[m[32m    function getViewportHeight() {[m
[32m+[m[32m        var height = window.innerHeight; // Safari, Opera[m
[32m+[m[32m        var mode = document.compatMode;[m
[32m+[m
[32m+[m[32m        if ( (mode || !$.support.boxModel) ) { // IE, Gecko[m
[32m+[m[32m            height = (mode == 'CSS1Compat') ?[m
[32m+[m[32m            document.documentElement.clientHeight : // Standards[m
[32m+[m[32m            document.body.clientHeight; // Quirks[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return height;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    $(window).scroll(function () {[m
[32m+[m[32m        var vpH = getViewportHeight(),[m
[32m+[m[32m            scrolltop = (document.documentElement.scrollTop ?[m
[32m+[m[32m                document.documentElement.scrollTop :[m
[32m+[m[32m                document.body.scrollTop),[m
[32m+[m[32m            elems = [];[m
[32m+[m[41m        [m
[32m+[m[32m        // naughty, but this is how it knows which elements to check for[m
[32m+[m[32m        $.each($.cache, function () {[m
[32m+[m[32m            if (this.events && this.events.inview) {[m
[32m+[m[32m                elems.push(this.handle.elem);[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        if (elems.length) {[m
[32m+[m[32m            $(elems).each(function () {[m
[32m+[m[32m                var $el = $(this),[m
[32m+[m[32m                    top = $el.offset().top,[m
[32m+[m[32m                    height = $el.height(),[m
[32m+[m[32m                    inview = $el.data('inview') || false;[m
[32m+[m
[32m+[m[32m                if (scrolltop > (top + height) || scrolltop + vpH < top) {[m
[32m+[m[32m                    if (inview) {[m
[32m+[m[32m                        $el.data('inview', false);[m
[32m+[m[32m                        $el.trigger('inview', [ false ]);[m[41m                        [m
[32m+[m[32m                    }[m
[32m+[m[32m                } else if (scrolltop < (top + height)) {[m
[32m+[m[32m                    if (!inview) {[m
[32m+[m[32m                        $el.data('inview', true);[m
[32m+[m[32m                        $el.trigger('inview', [ true ]);[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m[41m    [m
[32m+[m[32m    // kick the event to pick up any elements already in view.[m
[32m+[m[32m    // note however, this only works if the plugin is included after the elements are bound to 'inview'[m
[32m+[m[32m    $(function () {[m
[32m+[m[32m        $(window).scroll();[m
[32m+[m[32m    });[m
[32m+[m[32m})(jQuery);[m
