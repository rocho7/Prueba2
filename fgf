[32m+[m[32m(function($){[m[41m [m
[32m+[m[32m     $.fn.extend({[m[41m  [m
[32m+[m[32m         airport: function(array) {[m
[32m+[m			[32m$me = $(this);[m
[32m+[m			[32m/* Consider making runOnce configurable */[m
[32m+[m			[32mvar runOnce = true[m
[32m+[m			[32mvar self = $(this);[m
[32m+[m			[32mvar chars = ['a','b','c','d','e','f','g',' ','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z','-',"'",'&','.'];[m
[32m+[m			[32mvar longest = 0;[m
[32m+[m			[32mvar items = items2 = array.length;[m
[32m+[m			[32mvar flipTimer = null;[m
[32m+[m
[32m+[m			[32mfunction pad(a,b) { return a + new Array(b - a.length + 1).join(' '); }[m
[32m+[m[41m			[m
[32m+[m			[32m$(this).empty();[m
[32m+[m[41m			[m
[32m+[m			[32mwhile(items--)[m
[32m+[m				[32mif(array[items].length > longest) longest = array[items].length;[m
[32m+[m
[32m+[m			[32mwhile(items2--)[m
[32m+[m				[32marray[items2] = pad(array[items2],longest);[m
[32m+[m[41m				[m
[32m+[m			[32mspans = longest;[m
[32m+[m			[32mwhile(spans--)[m
[32m+[m				[32m$(this).prepend("<span class='c" + spans + "'></span>");[m
[32m+[m[41m				[m
[32m+[m[41m			[m
[32m+[m			[32mfunction testChar(a,b,c,d){[m
[32m+[m				[32mif(c >= array.length)[m
[32m+[m					[32mflipTimer = setTimeout(function() {[m[41m [m
[32m+[m					[32m  testChar(0,0,0,0);[m[41m [m
[32m+[m					[32m}, 1000);[m[41m				[m
[32m+[m				[32melse if(d >= longest)[m
[32m+[m					[32msetTimeout(function() {[m[41m [m
[32m+[m					[32m  testChar(0,0,c+1,0);[m[41m [m
[32m+[m					[32m  if (runOnce) {[m
[32m+[m					[32m    clearTimeout(flipTimer);[m
[32m+[m[32m                        $me.triggerHandler("airport_complete");[m
[32m+[m					[32m  }[m
[32m+[m					[32m}, 1000);[m
[32m+[m				[32melse {[m
[32m+[m					[32m$(self).find('.c'+a).html((chars[b]==" ")?"&nbsp;":chars[b]);[m
[32m+[m					[32msetTimeout(function() {[m
[32m+[m						[32mif(b > chars.length)[m
[32m+[m							[32mtestChar(a+1,0,c,d+1);[m
[32m+[m						[32melse if(chars[b] != array[c].substring(d,d+1).toLowerCase())[m
[32m+[m							[32mtestChar(a,b+1,c,d);[m
[32m+[m						[32melse[m
[32m+[m							[32mtestChar(a+1,0,c,d+1);[m
[32m+[m					[32m}, 15);[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m[41m			[m
[32m+[m			[32mtestChar(0,0,0,0);[m
[32m+[m[32m        }[m[41m [m
[32m+[m[32m    });[m[41m [m
[32m+[m[32m})(jQuery);[m
