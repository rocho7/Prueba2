[32m+[m
[32m+[m			[32mscrollOutside  : true,[m
[32m+[m
[32m+[m			[32m// Override some properties[m
[32m+[m			[32mindex   : 0,[m
[32m+[m			[32mtype    : null,[m
[32m+[m			[32mhref    : null,[m
[32m+[m			[32mcontent : null,[m
[32m+[m			[32mtitle   : null,[m
[32m+[m
[32m+[m			[32m// HTML templates[m
[32m+[m			[32mtpl: {[m
[32m+[m				[32mwrap     : '<div class="fancybox-wrap" tabIndex="-1"><div class="fancybox-skin"><div class="fancybox-outer"><div class="fancybox-inner"></div></div></div></div>',[m
[32m+[m				[32mimage    : '<img class="fancybox-image" src="{href}" alt="" />',[m
[32m+[m				[32miframe   : '<iframe id="fancybox-frame{rnd}" name="fancybox-frame{rnd}" class="fancybox-iframe" frameborder="0" vspace="0" hspace="0" webkitAllowFullScreen mozallowfullscreen allowFullScreen' + (IE ? ' allowtransparency="true"' : '') + '></iframe>',[m
[32m+[m				[32merror    : '<p class="fancybox-error">The requested content cannot be loaded.<br/>Please try again later.</p>',[m
[32m+[m				[32mcloseBtn : '<a title="Close" class="fancybox-item fancybox-close" href="javascript:;"></a>',[m
[32m+[m				[32mnext     : '<a title="Next" class="fancybox-nav fancybox-next" href="javascript:;"><span></span></a>',[m
[32m+[m				[32mprev     : '<a title="Previous" class="fancybox-nav fancybox-prev" href="javascript:;"><span></span></a>'[m
[32m+[m			[32m},[m
[32m+[m
[32m+[m			[32m// Properties for each animation type[m
[32m+[m			[32m// Opening fancyBox[m
[32m+[m			[32mopenEffect  : 'fade', // 'elastic', 'fade' or 'none'[m
[32m+[m			[32mopenSpeed   : 250,[m
[32m+[m			[32mopenEasing  : 'swing',[m
[32m+[m			[32mopenOpacity : true,[m
