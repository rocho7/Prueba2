[32m+[m			[32m$win = $(window);[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn $win.width() - 79; // offset for toggle link[m
[32m+[m	[32m}[m
[32m+[m	[32mfunction addScrollTopAnimation() {[m
[32m+[m
[32m+[m		[32mvar $scrolltop_link = $('.menu-item-back-to-top');[m
[32m+[m		[32m$scrolltop_link.on( 'click' ,  function ( ev ) {[m
[32m+[m			[32mev.preventDefault();[m
[32m+[m			[32m$( 'html, body' ).animate([m
[32m+[m				[32m{ scrollTop: 0 },[m
[32m+[m				[32m700,[m
[32m+[m				[32m'easeInCubic'[m
[32m+[m			[32m);[m
[32m+[m		[32m})[m
[32m+[m
[32m+[m		[32m// Hides the link initially[m
[32m+[m		[32m.data('hidden', 1).hide();[m
[32m+[m
[32m+[m		[32mvar scroll_event_fired = false;[m
[32m+[m		[32m$(window).on('scroll', function() {[m
[32m+[m			[32mscroll_event_fired = true;[m
[32m+[m		[32m});[m
[32m+[m		[32m/*[m
[32m+[m		[32mChecks every 300 ms if a scroll event has been fired.[m
[32m+[m		[32m*/[m
[32m+[m
[32m+[m		[32msetInterval( function() {[m
[32m+[m			[32mif( scroll_event_fired ) {[m
[32m+[m				[32m// Stop code below from being executed until the next scroll event.[m
[32m+[m				[32mscroll_event_fired = false;[m
[32m+[m				[32mvar is_hidden =  $scrolltop_link.data('hidden');[m
[32m+[m
[32m+[m				[32m/*[m
[32m+[m				[32mDisplay the scroll top link when the page is scrolled[m
[32m+[m				[32mdown the height of half a viewport from top,  Hide it otherwise.[m
[32m+[m				[32m*/[m
[32m+[m
[32m+[m				[32mif ( $( this ).scrollTop()  >  $( this ).height() / 2 ) {[m
[32m+[m					[32mif( is_hidden ) {[m
[32m+[m						[32m$scrolltop_link.slideDown(600).data('hidden', 0);[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m				[32melse {[m
[32m+[m					[32mif( !is_hidden ) {[m
[32m+[m						[32m$scrolltop_link.slideUp(600).data('hidden', 1);[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}, 300);[m
[32m+[m	[32m}[m
[32m+[m	[32mfunction animateArrow($icon, property, point_a, point_b) {}[m
[32m+[m[32m}(jQuery);[m
