[32m+[m			[32mTweenMax.to(elem.find('.production-shadow'), 2, {css:{'autoAlpha':1}});[m
[32m+[m		[32m},[m
[32m+[m		[32mthis.triggerSmarterBigger = function(smarterBigger) {[m
[32m+[m			[32mtimeline = new TimelineMax();[m
[32m+[m[41m       [m
[32m+[m[32m      for(i=0;i<6;i++) {[m
[32m+[m[32m        timeline.append( TweenMax.to(smarterBigger.find("img").eq(i), .5, {css:{'left':'584px'}}),-0.4 );[m
[32m+[m[32m      }[m
[32m+[m		[32m},[m
[32m+[m		[32mthis.triggerSquareGrid = function(gridType) {[m
[32m+[m			[32mgridType.addClass("transitioned");[m
[32m+[m			[32mif(!$('html').hasClass("csstransitions")) {[m
[32m+[m				[32mvar rightValue = 0;[m
[32m+[m				[32mvar topValue = 0;[m
[32m+[m				[32mgridType.find("div").each(function(index) {[m
[32m+[m					[32m$(this).data('leftValue',$(this).css('left'));[m
[32m+[m					[32m$(this).data('topValue',$(this).css('top'));[m
[32m+[m				[32m});[m
[32m+[m				[32mgridType.removeClass("transitioned");[m
[32m+[m				[32mgridType.find("div").each(function(index) {[m
[32m+[m					[32mTweenMax.to($(this), .75, {css:{'left':$(this).data('leftValue')+'px','top':$(this).data('topValue')+'px',delay:index*.25}});[m
[32m+[m				[32m});[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m		[32mthis.initAnimations = function() {[m
[32m+[m			[32m$('.product-images').each( function() {[m
[32m+[m				[32m$.getScript($(this).data('animationfile'), _this.onScriptLoaded).fail(function(jqxhr, settings, exception){[m
[32m+[m[41m					[m
[32m+[m				[32m});[m
[32m+[m			[32m});[m
[32m+[m		[32m},[m
[32m+[m		[32mthis.onPlayButtonClick = function(evt) {[m
