[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction animatePage($currPage, outClass){[m
[32m+[m
[32m+[m		[32mif(timer>0) clearTimeout(timer);[m
[32m+[m		[32mtimer = setTimeout(nextPage, waittime);[m
[32m+[m
[32m+[m		[32mvar windowW = $(window).width();[m
[32m+[m		[32mvar windowH = $(window).height();[m
[32m+[m
[32m+[m
[32m+[m		[32mvar $nextPage = $pages.eq( current ).addClass( 'slide-current' ).addClass('slide-ontop');[m
[32m+[m
[32m+[m		[32mif( isAnimating ) {[m
[32m+[m			[32minSequence.pause();[m
[32m+[m			[32monInAnimationEnd($nextPage);[m
[32m+[m		[32m}[m
[32m+[m		[32m// shuffle the current page out with CSS animation[m
[32m+[m[41m		[m
[32m+[m		[32mif($currPage){[m
[32m+[m			[32mif(Modernizr.csstransforms3d)[m
[32m+[m			[32m{[m
[32m+[m				[32m$currPage.addClass( outClass ).on( animEndEventName, function() {[m
[32m+[m					[32m$currPage.off( animEndEventName );[m
[32m+[m					[32monOutAnimationEnd($currPage);[m
[32m+[m				[32m});[m
[32m+[m			[32m}[m
[32m+[m			[32melse{[m
[32m+[m				[32mTweenLite.to($currPage, 0.5, {top:windowW, onComplete:function(){[m
[32m+[m					[32m$currPage.off( animEndEventName );[m
[32m+[m					[32monOutAnimationEnd($currPage);[m
[32m+[m				[32m}});[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m[41m		[m
[32m+[m		[32misAnimating = true;[m
[32m+[m
[32m+[m		[32m// bring next page in: /reset css /do sequenced animation via TweenLite[m
[32m+[m		[32minSequence = new TimelineLite({onComplete:onInAnimationEnd,onCompleteParams:[$nextPage]});[m
[32m+[m		[32mif(!$currPage || $currPage.data('type')=='about' || $nextPage.data('type')=='about'  ) inSequence.from($nextPage.find('.headline'), 0.5, {css:{width:0}});[m
[32m+[m
[32m+[m		[32mif($nextPage.data('type') == 'about')[m
[32m+[m		[32m{[m
[32m+[m			[32minSequence.from( $nextPage.find('.header-top'), 2, {css:{rotationX:'-95deg'}, ease:Elastic.easeOut, onStart:function(){$nextPage.find('.header-top').css({'visibility':'visible'});}});[m
[32m+[m			[32minSequence.from( $nextPage.find('.header-bottom'), 2, {css:{rotationX:'-95deg'}, ease:Elastic.easeOut, onStart:function(){$nextPage.find('.header-bottom').css({'visibility':'visible'});}}, '-=1.5');[m
[32m+[m		[32m}[m
[32m+[m		[32melse if($nextPage.data('type') == 'work')[m
[32m+[m		[32m{[m
[32m+[m			[32minSequence.from( $nextPage.find('.title'), 2, {x:-$nextPage.find('.title').width(), ease:Expo.easeOut});[m
[32m+[m			[32minSequence.from( $nextPage.find('.subtitle'), 2, {x:windowW, ease:Expo.easeOut}, '-=1.5');[m
[32m+[m			[32minSequence.from( $nextPage.find('.featured-image'), 2, {x:-windowW, ease:Expo.easeOut}, '-=1.5');[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32minSequence.play();[m
[32m+[m
[32m+[m		[32msectionChangeCallback.call(this,$nextPage.data('type'));[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction onInAnimationEnd( $inpage ) {[m
[32m+[m		[32m$inpage.attr( 'class', $inpage.data( 'originalClassList' ) + ' slide-current' );[m
[32m+[m		[32misAnimating = false;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction onOutAnimationEnd( $outpage ) {[m
[32m+[m		[32mresetPage( $outpage);[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction resetPage( $outpage ) {[m
[32m+[m		[32mif($outpage){[m
[32m+[m			[32m$outpage.attr( 'class', $outpage.data( 'originalClassList' ) );[m
[32m+[m			[32m$outpage.find('.header-top').css({'visibility':'hidden', 'transform':'none'});[m
[32m+[m			[32m$outpage.find('.header-bottom').css({'visibility':'hidden', 'transform':'none'});[m
[32m+[m
[32m+[m			[32m$outpage.find('.title').css({'transform':'none'});[m
[32m+[m			[32m$outpage.find('.subtitle').css({'transform':'none'});[m
[32m+[m			[32m$outpage.find('.featured-image').css({'transform':'none'});[m
[32m+[m
[32m+[m			[32m$outpage.css('top',0);[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m
[32m+[m	[32mfunction getCurrentType(){[m
[32m+[m		[32mreturn $pages.eq( current ).data('type');[m
[32m+[m	[32m}[m
[32m+[m	[32m//init();[m
[32m+[m
[32m+[m	[32mreturn { init:init,[m
[32m+[m						[32mgetCurrentType:getCurrentType };[m
[32m+[m[32m};[m
\ No newline at end of file[m
