[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction onEndAnimation( $outpage, $inpage ) {[m
[32m+[m		[32mendCurrPage = false;[m
[32m+[m		[32mendNextPage = false;[m
[32m+[m		[32m//resetPage( $outpage, $inpage );[m
[32m+[m		[32misAnimating = false;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction resetPage( $outpage, $inpage ) {[m
[32m+[m		[32m$outpage.attr( 'class', $outpage.data( 'originalClassList' ) );[m
[32m+[m		[32m$inpage.attr( 'class', $inpage.data( 'originalClassList' ) + ' slide-current' );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m//init();[m
[32m+[m
[32m+[m	[32mreturn { init : init };[m
[32m+[m
[32m+[m[32m};[m
