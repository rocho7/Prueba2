[32m+[m					[32m40 : 'up'    // down arrow[m
[32m+[m				[32m},[m
[32m+[m				[32mprev : {[m
[32m+[m					[32m8  : 'right',  // backspace[m
[32m+[m					[32m33 : 'down',   // page up[m
[32m+[m					[32m37 : 'right',  // left arrow[m
[32m+[m					[32m38 : 'down'    // up arrow[m
[32m+[m				[32m},[m
[32m+[m				[32mclose  : [27], // escape key[m
[32m+[m				[32mplay   : [32], // space - start/stop slideshow[m
[32m+[m				[32mtoggle : [70]  // letter "f" - toggle fullscreen[m
[32m+[m			[32m},[m
[32m+[m
[32m+[m			[32mdirection : {[m
