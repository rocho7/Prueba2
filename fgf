[32m+[m[41m        [m		[32m    $me.width(windowWidth);[m
[32m+[m[41m        [m		[32m  } else {[m
[32m+[m[41m        [m		[32m    $me.height(windowWidth / imgRatio);[m
[32m+[m[41m        [m		[32m    $me.css("width","");[m
[32m+[m[41m        		    [m
[32m+[m[41m        [m		[32m  }[m
[32m+[m[41m        		  [m
[32m+[m[41m        [m		[32m}[m
[32m+[m[41m        		[m
[32m+[m[41m        [m		[32mfunction _resize(withAnimation) {[m
[32m+[m[32m                    if($me.hasClass("video-player-container")) debug("resizing video");[m
[32m+[m[41m        [m			[32mif($me.hasClass("fullscreen-video")) return;[m
[32m+[m[41m        [m			[32mif(withAnimation) resize();[m
[32m+[m[41m        [m			[32mif(!imgRatio && checkImgRatioTries < 3) {[m
[32m+[m[41m        [m				[32mcheckImgRatioTries++;[m
[32m+[m[41m        [m				[32m_onImageLoaded();[m
[32m+[m[41m        [m				[32mreturn;[m
[32m+[m[41m        [m			[32m}[m
[32m+[m
[32m+[m[41m        [m			[32mif(!canResize) return;[m
[32m+[m[41m        [m			[32mtotalWidth = windowWidth;[m
[32m+[m[41m        [m			[32mtotalHeight = windowHeight;[m
[32m+[m
[32m+[m[32m                    if($me.hasClass("video-player-container")) {[m
[32m+[m[32m                        debug(totalWidth)[m
[32m+[m[32m                        debug(totalHeight)[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[41m        [m			[32mif(options.maxWidth > -1 && totalWidth > options.maxWidth) totalWidth = options.maxWidth;[m
[32m+[m[41m        [m			[32mif(options.maxHeight > -1 && totalHeight > options.maxHeight) totalHeight = options.maxHeight;[m
[32m+[m[41m        			[m
[32m+[m[41m        [m			[32mresizedWidth = totalWidth;[m
[32m+[m[41m        [m			[32mresizedHeight = Math.round(resizedWidth / imgRatio);[m
[32m+[m[41m        [m			[32mif(!widthOnly) {[m
[32m+[m	[41m        [m			[32mif(useMaxSize) {[m
[32m+[m		[41m        [m			[32mif(resizedHeight < totalHeight) {[m
[32m+[m		[41m        [m			[32m    resizedHeight = totalHeight;[m
[32m+[m		[41m        [m			[32m    resizedWidth = Math.round(resizedHeight * imgRatio);[m
[32m+[m		[41m        [m			[32m}[m
[32m+[m	[41m        [m			[32m} else {[m
[32m+[m	[41m        [m				[32mif(resizedHeight > totalHeight) {[m
[32m+[m	[41m        [m					[32mresizedHeight = totalHeight;[m
[32m+[m	[41m        [m					[32mresizedWidth = resizedHeight * imgRatio;[m
[32m+[m	[41m        [m				[32m}[m
[32m+[m	[41m        [m			[32m}[m
[32m+[m[41m        [m			[32m}[m
[32m+[m
[32m+[m[41m        [m			[32m($me.css("position")=="absolute") ? offsetTopAttribute = "top" : offsetTopAttribute = "margin-top";[m
[32m+[m[41m        [m			[32m//offsetTopAttribute = "top";[m
[32m+[m[41m        [m			[32mresizedOffsetTop = -(resizedHeight - windowHeight)*.5;[m
[32m+[m[41m        [m			[32mif(options.centeredY) $me.css(offsetTopAttribute, resizedOffsetTop+"px");[m
[32m+[m[41m        			[m
[32m+[m[41m        [m			[32mresizedOffsetLeft = -(resizedWidth - windowWidth)*.5;[m
[32m+[m[41m        [m			[32m($me.css("position")=="absolute") ? offsetLeftAttribute = "left" : offsetLeftAttribute = "margin-left";[m
[32m+[m[41m        [m			[32m//offsetLeftAttribute = "margin-left";[m
[32m+[m[41m        [m			[32m/*[m
[32m+[m[41m        [m			[32mif($me.parent().hasClass("fullscreen") && resizedHeight > $.Window.windowHeight) {[m
[32m+[m[41m        [m			[32m    resizedHeight = $.Window.windowHeight;[m
[32m+[m[41m        [m			[32m    resizedWidth = Math.round(resizedHeight * imgRatio);[m
[32m+[m[41m        [m			[32m    resizedOffsetLeft = $.Window.windowWidth - resizedWidth();[m
[32m+[m[41m        [m			[32m}[m
[32m+[m[41m        [m			[32m*/[m
[32m+[m					[32mif(withAnimation) {[m
[32m+[m						[32mif(options.centeredX) {[m
[32m+[m							[32m(offsetLeftAttribute=="left") ? $me.animate({width:resizedWidth+'px',height:resizedHeight+'px',left:resizedOffsetLeft+"px"},animationSpeed) : $me.animate({width:resizedWidth+'px',height:resizedHeight+'px',marginLeft:resizedOffsetLeft+"px"},animationSpeed);[m
[32m+[m						[32m} else {[m
[32m+[m							[32m$me.animate({width:resizedWidth+'px',height:resizedHeight+'px'},animationSpeed);[m
[32m+[m						[32m}[m
[32m+[m						[32m//$me.width(resizedWidth);[m
[32m+[m						[32m//$me.height(resizedHeight);[m
[32m+[m						[32m//$me.attr("width","");[m
[32m+[m						[32m//$me.attr("height","");[m
[32m+[m					[32m} else {[m
[32m+[m						[32mif(options.centeredX) $me.css(offsetLeftAttribute, resizedOffsetLeft+"px");[m
[32m+[m	[41m        [m			[32m$me.width(resizedWidth);[m
[32m+[m	[41m        [m			[32m$me.height(resizedHeight);[m
[32m+[m	[41m        [m			[32m//add the width & height attributes to the tag if it's set to do so and if it's an image tag[m
[32m+[m	[41m        [m			[32mif($me.is("img") && options.addWidthHeightAttributes) {[m[41m [m
[32m+[m	[41m        [m				[32m$me.attr("width",resizedWidth);[m
[32m+[m	[41m        [m				[32m$me.attr("height",resizedHeight);[m
[32m+[m	[41m        [m			[32m}[m
[32m+[m	[41m        [m		[32m}[m
[32m+[m[41m        			[m
[32m+[m[41m        [m		[32m}[m
[32m+[m[41m        [m	[32m});[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m[32m})(jQuery);[m
[1mdiff --git a/gitnn/Advertising/jquery.superscrollorama.js b/gitnn/Advertising/jquery.superscrollorama.js[m
[1mnew file mode 100644[m
[1mindex 0000000..54f52ee[m
[1m--- /dev/null[m
[1m+++ b/gitnn/Advertising/jquery.superscrollorama.js[m
[36m@@ -0,0 +1,219 @@[m
[32m+[m[32m/*[m
[32m+[m	[32mSUPERSCROLLORAMA - The jQuery plugin for doing scroll animations[m
[32m+[m	[32mby John Polacek (@johnpolacek)[m
[32m+[m[41m	[m
[32m+[m	[32mPowered by the Greensock Tweening Platform[m
[32m+[m	[32mhttp://www.greensock.com[m
[32m+[m	[32mGreensock License info at http://www.greensock.com/licensing/[m
[32m+[m[41m	[m
[32m+[m	[32mDual licensed under MIT and GPL.[m
[32m+[m[32m*/[m
[32m+[m
[32m+[m[32m(function($) {[m
[32m+[m
[32m+[m	[32m$.superscrollorama = function(options) {[m
[32m+[m[41m		[m
[32m+[m		[32mvar superscrollorama = this;[m
[32m+[m		[32mvar defaults = {isVertical:true};[m
[32m+[m		[32msuperscrollorama.settings = $.extend({}, defaults, options);[m
[32m+[m
[32m+[m[41m		[m
[32m+[m		[32m// PRIVATE VARS[m
[32m+[m[41m		[m
[32m+[m		[32mvar animObjects = [],[m
[32m+[m			[32mpinnedObjects = [],[m
[32m+[m			[32mdidScrollCheck = false;[m
[32m+[m[41m		[m
[32m+[m		[32m// PRIVATE FUNCTIONS[m
[32m+[m
[32m+[m		[32mfunction init() {[m
[32m+[m			[32m// scroll to top of page[m
[32m+[m			[32m$('html, body').animate({ scrollTop: 0 }, 0);[m
[32m+[m[41m			[m
[32m+[m			[32m$(window).scroll(function() {[m
[32m+[m				[32mdidScrollCheck = true;[m
[32m+[m			[32m});[m
[32m+[m			[32m(function loop() {[m
[32m+[m[32m                if (didScrollCheck) {[m
[32m+[m					[32mcheckScrollAnim();[m
[32m+[m					[32mdidScrollCheck = false;[m
[32m+[m				[32m}[m
[32m+[m[32m                setTimeout(loop, 100); // loop calls itself and runs, then waits 100 miliseconds and runs again[m
[32m+[m[32m            }());[m
[32m+[m		[32m}[m
[32m+[m[41m		[m
[32m+[m		[32mfunction checkScrollAnim() {[m
[32m+[m			[32mvar currScrollPoint = superscrollorama.settings.isVertical ? $(window).scrollTop() : $(window).scrollLeft();[m
[32m+[m			[32mvar offsetAdjust = superscrollorama.settings.isVertical ? -$(window).height()/2 : -$(window).width()/2;[m
[32m+[m			[32mvar i, startPoint, endPoint;[m
[32m+[m[41m			[m
[32m+[m			[32m// check all animObjects[m
[32m+[m			[32mvar numAnim = animObjects.length;[m
[32m+[m			[32mfor (i=0; i<numAnim; i++) {[m
[32m+[m				[32mvar animObj = animObjects[i],[m
[32m+[m					[32mtarget = animObj.target,[m
[32m+[m					[32moffset = animObj.offset;[m
[32m+[m[41m			[m
[32m+[m				[32mif (typeof(target) === 'string') {[m
[32m+[m					[32mstartPoint = superscrollorama.settings.isVertical ? $(target).offset().top : $(target).offset().left;[m
[32m+[m					[32moffset += offsetAdjust;[m
[32m+[m				[32m} else if (typeof(target) === 'number')	{[m
[32m+[m					[32mstartPoint = target;[m
[32m+[m				[32m} else {[m
[32m+[m					[32mstartPoint = superscrollorama.settings.isVertical ? target.offset().top : target.offset().left;[m
[32m+[m					[32moffset += offsetAdjust;[m
[32m+[m				[32m}[m
[32m+[m[41m				[m
[32m+[m				[32mstartPoint += offset;[m
[32m+[m				[32mendPoint = animObj.dur ? startPoint + animObj.dur : startPoint;[m
[32m+[m[41m				[m
[32m+[m				[32mif ((currScrollPoint > startPoint && currScrollPoint < endPoint) && animObj.state !== 'TWEENING') {[m
[32m+[m					[32m// if it should be TWEENING and isn't..[m
[32m+[m					[32manimObj.state = 'TWEENING';[m
[32m+[m					[32manimObj.start = startPoint;[m
[32m+[m					[32manimObj.end = endPoint;[m
[32m+[m					[32manimObj.tween.progress((currScrollPoint - animObj.start)/(animObj.end - animObj.start)).pause();[m
[32m+[m				[32m} else if (currScrollPoint < startPoint && animObj.state !== 'BEFORE') {[m
[32m+[m					[32m// if it should be at the BEFORE tween state and isn't..[m
[32m+[m					[32manimObj.tween.reverse();[m
[32m+[m					[32manimObj.state = 'BEFORE';[m
[32m+[m				[32m} else if (currScrollPoint > endPoint && animObj.state !== 'AFTER') {[m
[32m+[m					[32m// if it should be at the AFTER tween state and isn't..[m
[32m+[m					[32manimObj.tween.play();[m
[32m+[m					[32manimObj.state = 'AFTER';[m
[32m+[m				[32m} else if (animObj.state === 'TWEENING') {[m
[32m+[m					[32m// if it is TWEENING..[m
[32m+[m					[32manimObj.tween.progress((currScrollPoint - animObj.start)/(animObj.end - animObj.start)).pause();[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m[41m				[m
[32m+[m			[32m// check all pinned elements[m
[32m+[m			[32mvar numPinned = pinnedObjects.length;[m
[32m+[m			[32mfor (i=0; i<numPinned; i++) {[m
[32m+[m				[32mvar pinObj = pinnedObjects[i];[m
[32m+[m				[32mvar el = pinObj.el;[m
[32m+[m				[32mvar elHeight = el.outerHeight();[m
[32m+[m[41m				[m
[32m+[m				[32m// should object be pinned?[m
[32m+[m				[32mif (pinObj.state != 'PINNED') {[m
[32m+[m[41m					[m
[32m+[m					[32mstartPoint = pinObj.spacer ?[m
[32m+[m						[32msuperscrollorama.settings.isVertical ? pinObj.spacer.offset().top : pinObj.spacer.offset().left :[m
[32m+[m						[32msuperscrollorama.settings.isVertical ? el.offset().top : el.offset().left;[m
[32m+[m[41m					[m
[32m+[m					[32mstartPoint += pinObj.offset;[m
[32m+[m
[32m+[m					[32mendPoint = startPoint + pinObj.dur;[m
[32m+[m[41m					[m
[32m+[m					[32mif (currScrollPoint > startPoint && currScrollPoint < endPoint) {[m
[32m+[m						[32m// pin it[m
[32m+[m						[32mpinObj.state = 'PINNED';[m
[32m+[m[41m						[m
[32m+[m						[32m// set original position value for unpinning[m
[32m+[m						[32mpinObj.origPositionVal = superscrollorama.settings.isVertical ? el.css('top') : el.css('left');[m
[32m+[m						[32mif (pinObj.origPositionVal === 'auto')[m
[32m+[m							[32mpinObj.origPositionVal = 0;[m
[32m+[m						[32melse[m
[32m+[m							[32mpinObj.origPositionVal = parseInt(pinObj.origPositionVal, 10);[m
[32m+[m[41m						[m
[32m+[m						[32m// change to fixed position[m
[32m+[m						[32mel.css('position','fixed');[m
[32m+[m						[32mif (superscrollorama.settings.isVertical)[m
[32m+[m							[32mel.css('top', -pinObj.offset);[m
[32m+[m						[32melse[m
[32m+[m							[32mel.css('left', -pinObj.offset);[m
[32m+[m[41m						[m
[32m+[m						[32mpinObj.pinStart = startPoint;[m
[32m+[m						[32mpinObj.pinEnd = endPoint;[m
[32m+[m
[32m+[m						[32mif (pinObj.spacer)[m
[32m+[m							[32mpinObj.spacer.css('height', pinObj.dur + elHeight);[m
[32m+[m
[32m+[m						[32mif (pinObj.onPin)[m
[32m+[m							[32mpinObj.onPin();[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m				[32m// Check to see if object should be unpinned[m
[32m+[m				[32m} else {[m
[32m+[m[41m					[m
[32m+[m					[32mif (currScrollPoint < pinObj.pinStart || currScrollPoint > pinObj.pinEnd) {[m
[32m+[m[41m						[m
[32m+[m						[32m// unpin it[m
[32m+[m						[32mpinObj.state = currScrollPoint < pinObj.pinStart ? 'BEFORE' : 'AFTER';[m
[32m+[m						[32mif(pinObj.anim&&pinObj.state === 'BEFORE'){[m
[32m+[m							[32mpinObj.anim.progress(0);[m
[32m+[m						[32m}else if(pinObj.anim&&pinObj.state === 'AFTER'){[m
[32m+[m							[32mpinObj.anim.progress(1);[m
[32m+[m						[32m}[m
[32m+[m						[32m// revert to original position value[m
[32m+[m						[32mel.css('position',pinObj.origPosition);[m
[32m+[m						[32mif (superscrollorama.settings.isVertical)[m
[32m+[m							[32mel.css('top', pinObj.origPositionVal);[m
[32m+[m						[32melse[m
[32m+[m							[32mel.css('left', pinObj.origPositionVal);[m
[32m+[m[41m						[m
[32m+[m						[32mif (pinObj.spacer)[m
[32m+[m							[32mpinObj.spacer.css('height', currScrollPoint < pinObj.pinStart ? 0 : pinObj.dur);[m
[32m+[m
[32m+[m						[32mif (pinObj.onUnpin)[m
[32m+[m							[32mpinObj.onUnpin();[m
[32m+[m					[32m}[m
[32m+[m					[32melse if (pinObj.anim) {[m
[32m+[m						[32m// do animation[m
[32m+[m						[32mpinObj.anim.progress((currScrollPoint - pinObj.pinStart)/(pinObj.pinEnd - pinObj.pinStart));[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m[41m		[m
[32m+[m		[32m// PUBLIC FUNCTIONS[m
[32m+[m		[32msuperscrollorama.addTween = function(target, tween, dur, offset) {[m
[32m+[m[41m			[m
[32m+[m			[32mtween.pause();[m
[32m+[m[41m						[m
[32m+[m			[32manimObjects.push({[m
[32m+[m				[32mtarget:target,[m
[32m+[m				[32mtween: tween,[m
[32m+[m				[32moffset: offset || 0,[m
[32m+[m				[32mdur: dur || 0,[m
[32m+[m				[32mstate:'BEFORE'[m
[32m+[m			[32m});[m
[32m+[m[41m      [m
[32m+[m			[32mreturn superscrollorama;[m
[32m+[m		[32m};[m
[32m+[m[41m		[m
[32m+[m		[32msuperscrollorama.pin = function(el, dur, vars) {[m
[32m+[m			[32mif (typeof(el) === 'string') el = $(el);[m
[32m+[m			[32mif (vars.anim) vars.anim.pause();[m
[32m+[m[41m			[m
[32m+[m			[32m// create wrapper for pinned elements that aren't absolute or fixed position[m
[32m+[m			[32mvar pinSpacer = null;[m
[32m+[m			[32mif (el.css('position') === 'relative' || el.css('position') === 'static') {[m
[32m+[m				[32mpinSpacer = $('<div class="pin-spacer"></div>');[m
[32m+[m				[32mel.before(pinSpacer);[m
[32m+[m			[32m}[m
[32m+[m[41m			[m
[32m+[m			[32mpinnedObjects.push({[m
[32m+[m				[32mel:el,[m
[32m+[m				[32mstate:'BEFORE',[m
[32m+[m				[32mdur:dur,[m
[32m+[m				[32moffset: vars.offset || 0,[m
[32m+[m				[32manim:vars.anim,[m
[32m+[m				[32morigPosition:el.css('position'),[m
[32m+[m				[32mspacer:pinSpacer,[m
[32m+[m				[32monPin:vars.onPin,[m
[32m+[m				[32monUnpin:vars.onUnpin[m
[32m+[m			[32m});[m
[32m+[m[41m      [m
[32m+[m			[32mreturn superscrollorama;[m
[32m+[m		[32m};[m
[32m+[m[41m		[m
[32m+[m[41m		[m
[32m+[m		[32m// INIT[m
[32m+[m		[32minit();[m
[32m+[m[41m		[m
[32m+[m		[32mreturn superscrollorama;[m
[32m+[m	[32m};[m
[32m+[m[41m     [m
[32m+[m[32m})(jQuery);[m
