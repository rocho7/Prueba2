[32m+[m[32m(function (window, document, $, undefined) {[m
[32m+[m	[32m"use strict";[m
[32m+[m
[32m+[m	[32mvar W = $(window),[m
[32m+[m		[32mD = $(document),[m
[32m+[m		[32mF = $.fancybox = function () {[m
[32m+[m			[32mF.open.apply( this, arguments );[m
[32m+[m		[32m},[m
[32m+[m		[32mIE =  navigator.userAgent.match(/msie/),[m
[32m+[m		[32mdidUpdate = null,[m
[32m+[m		[32misTouch	  = document.createTouch !== undefined,[m
[32m+[m
[32m+[m		[32misQuery	= function(obj) {[m
[32m+[m			[32mreturn obj && obj.hasOwnProperty && obj instanceof $;[m
[32m+[m		[32m},[m
[32m+[m		[32misString = function(str) {[m
[32m+[m			[32mreturn str && $.type(str) === "string";[m
[32m+[m		[32m},[m
[32m+[m		[32misPercentage = function(str) {[m
[32m+[m			[32mreturn isString(str) && str.indexOf('%') > 0;[m
[32m+[m		[32m},[m
[32m+[m		[32misScrollable = function(el) {[m
[32m+[m			[32mreturn (el && !(el.style.overflow && el.style.overflow === 'hidden') && ((el.clientWidth && el.scrollWidth > el.clientWidth) || (el.clientHeight && el.scrollHeight > el.clientHeight)));[m
[32m+[m		[32m},[m
[32m+[m		[32mgetScalar = function(orig, dim) {[m
[32m+[m			[32mvar value = parseInt(orig, 10) || 0;[m
[32m+[m
[32m+[m			[32mif (dim && isPercentage(orig)) {[m
[32m+[m				[32mvalue = F.getViewport()[ dim ] / 100 * value;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mreturn Math.ceil(value);[m
[32m+[m		[32m},[m
[32m+[m		[32mgetValue = function(value, dim) {[m
[32m+[m			[32mreturn getScalar(value, dim) + 'px';[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m	[32m$.extend(F, {[m
[32m+[m		[32m// The current version of fancyBox[m
[32m+[m		[32mversion: '2.1.4',[m
[32m+[m
[32m+[m		[32mdefaults: {[m
[32m+[m			[32mpadding : 15,[m
[32m+[m			[32mmargin  : 20,[m
[32m+[m
