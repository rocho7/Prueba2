[32m+[m
[32m+[m[32m!function ($) {[m
[32m+[m
[32m+[m[32m  "use strict"; // jshint ;_;[m
[32m+[m
[32m+[m
[32m+[m[32m /* CAROUSEL CLASS DEFINITION[m
[32m+[m[32m  * ========================= */[m
[32m+[m
[32m+[m[32m  var Carousel = function (element, options) {[m
[32m+[m[32m    this.$element = $(element)[m
[32m+[m[32m    this.options = options[m
[32m+[m[32m    this.options.slide && this.slide(this.options.slide)[m
[32m+[m[32m    this.options.pause == 'hover' && this.$element[m
[32m+[m[32m      .on('mouseenter', $.proxy(this.pause, this))[m
[32m+[m[32m      .on('mouseleave', $.proxy(this.cycle, this))[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Carousel.prototype = {[m
[32m+[m
[32m+[m[32m    cycle: function (e) {[m
[32m+[m[32m      if (!e) this.paused = false[m
[32m+[m[32m      this.options.interval[m
[32m+[m[32m        && !this.paused[m
[32m+[m[32m        && (this.interval = setInterval($.proxy(this.next, this), this.options.interval))[m
[32m+[m[32m      return this[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , to: function (pos) {[m
[32m+[m[32m      var $active = this.$element.find('.active')[m
[32m+[m[32m        , children = $active.parent().children()[m
[32m+[m[32m        , activePos = children.index($active)[m
[32m+[m[32m        , that = this[m
[32m+[m
[32m+[m[32m      if (pos > (children.length - 1) || pos < 0) return[m
[32m+[m
[32m+[m[32m      if (this.sliding) {[m
[32m+[m[32m        return this.$element.one('slid', function () {[m
[32m+[m[32m          that.to(pos)[m
[32m+[m[32m        })[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (activePos == pos) {[m
[32m+[m[32m        return this.pause().cycle()[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return this.slide(pos > activePos ? 'next' : 'prev', $(children[pos]))[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , pause: function (e) {[m
[32m+[m[32m      if (!e) this.paused = true[m
[32m+[m[32m      clearInterval(this.interval)[m
[32m+[m[32m      this.interval = null[m
[32m+[m[32m      return this[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , next: function () {[m
[32m+[m[32m      if (this.sliding) return[m
[32m+[m[32m      return this.slide('next')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , prev: function () {[m
[32m+[m[32m      if (this.sliding) return[m
[32m+[m[32m      return this.slide('prev')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , slide: function (type, next) {[m
[32m+[m[32m      var $active = this.$element.find('.active')[m
[32m+[m[32m        , $next = next || $active[type]()[m
[32m+[m[32m        , isCycling = this.interval[m
[32m+[m[32m        , direction = type == 'next' ? 'left' : 'right'[m
[32m+[m[32m        , fallback  = type == 'next' ? 'first' : 'last'[m
[32m+[m[32m        , that = this[m
[32m+[m[32m        , e = $.Event('slide')[m
[32m+[m
[32m+[m[32m      this.sliding = true[m
[32m+[m
[32m+[m[32m      isCycling && this.pause()[m
[32m+[m
[32m+[m[32m      $next = $next.length ? $next : this.$element.find('.item')[fallback]()[m
[32m+[m
[32m+[m[32m      if ($next.hasClass('active')) return[m
[32m+[m
[32m+[m[32m      if ($.support.transition && this.$element.hasClass('slide')) {[m
[32m+[m[32m        this.$element.trigger(e)[m
[32m+[m[32m        if (e.isDefaultPrevented()) return[m
[32m+[m[32m        $next.addClass(type)[m
[32m+[m[32m        $next[0].offsetWidth // force reflow[m
[32m+[m[32m        $active.addClass(direction)[m
[32m+[m[32m        $next.addClass(direction)[m
[32m+[m[32m        this.$element.one($.support.transition.end, function () {[m
[32m+[m[32m          $next.removeClass([type, direction].join(' ')).addClass('active')[m
[32m+[m[32m          $active.removeClass(['active', direction].join(' '))[m
[32m+[m[32m          that.sliding = false[m
[32m+[m[32m          setTimeout(function () { that.$element.trigger('slid') }, 0)[m
[32m+[m[32m        })[m
[32m+[m[32m      } else {[m
[32m+[m[32m        this.$element.trigger(e)[m
[32m+[m[32m        if (e.isDefaultPrevented()) return[m
[32m+[m[32m        $active.removeClass('active')[m
[32m+[m[32m        $next.addClass('active')[m
[32m+[m[32m        this.sliding = false[m
[32m+[m[32m        this.$element.trigger('slid')[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      isCycling && this.cycle()[m
[32m+[m
[32m+[m[32m      return this[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* CAROUSEL PLUGIN DEFINITION[m
[32m+[m[32m  * ========================== */[m
[32m+[m
[32m+[m[32m  $.fn.carousel = function (option) {[m
[32m+[m[32m    return this.each(function () {[m
[32m+[m[32m      var $this = $(this)[m
[32m+[m[32m        , data = $this.data('carousel')[m
[32m+[m[32m        , options = $.extend({}, $.fn.carousel.defaults, typeof option == 'object' && option)[m
[32m+[m[32m      if (!data) $this.data('carousel', (data = new Carousel(this, options)))[m
[32m+[m[32m      if (typeof option == 'number') data.to(option)[m
[32m+[m[32m      else if (typeof option == 'string' || (option = options.slide)) data[option]()[m
[32m+[m[32m      else if (options.interval) data.cycle()[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.carousel.defaults = {[m
[32m+[m[32m    interval: 5000[m
[32m+[m[32m  , pause: 'hover'[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.carousel.Constructor = Carousel[m
[32m+[m
[32m+[m
[32m+[m[32m /* CAROUSEL DATA-API[m
[32m+[m[32m  * ================= */[m
[32m+[m
[32m+[m[32m  $(function () {[m
[32m+[m[32m    $('body').on('click.carousel.data-api', '[data-slide]', function ( e ) {[m
[32m+[m[32m      var $this = $(this), href[m
[32m+[m[32m        , $target = $($this.attr('data-target') || (href = $this.attr('href')) && href.replace(/.*(?=#[^\s]+$)/, '')) //strip for ie7[m
[32m+[m[32m        , options = !$target.data('modal') && $.extend({}, $target.data(), $this.data())[m
[32m+[m[32m      $target.carousel(options)[m
[32m+[m[32m      e.preventDefault()[m
[32m+[m[32m    })[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m}(window.jQuery);[m
