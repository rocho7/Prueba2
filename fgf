[32m+[m			[32mcloseSpeed   : 250,[m
[32m+[m			[32mcloseEasing  : 'swing',[m
[32m+[m			[32mcloseOpacity : true,[m
[32m+[m			[32mcloseMethod  : 'zoomOut',[m
[32m+[m
[32m+[m			[32m// Changing next gallery item[m
[32m+[m			[32mnextEffect : 'elastic', // 'elastic', 'fade' or 'none'[m
[32m+[m			[32mnextSpeed  : 250,[m
[32m+[m			[32mnextEasing : 'swing',[m
[32m+[m			[32mnextMethod : 'changeIn',[m
[32m+[m
[32m+[m			[32m// Changing previous gallery item[m
[32m+[m			[32mprevEffect : 'elastic', // 'elastic', 'fade' or 'none'[m
[32m+[m			[32mprevSpeed  : 250,[m
[32m+[m			[32mprevEasing : 'swing',[m
[32m+[m			[32mprevMethod : 'changeOut',[m
[32m+[m
[32m+[m			[32m// Enable default helpers[m
[32m+[m			[32mhelpers : {[m
[32m+[m				[32moverlay : true,[m
[32m+[m				[32mtitle   : true[m
[32m+[m			[32m},[m
[32m+[m
[32m+[m			[32m// Callbacks[m
[32m+[m			[32monCancel     : $.noop, // If canceling[m
[32m+[m			[32mbeforeLoad   : $.noop, // Before loading[m
[32m+[m			[32mafterLoad    : $.noop, // After loading[m
[32m+[m			[32mbeforeShow   : $.noop, // Before changing in current item[m
[32m+[m			[32mafterShow    : $.noop, // After opening[m
[32m+[m			[32mbeforeChange : $.noop, // Before changing gallery item[m
[32m+[m			[32mbeforeClose  : $.noop, // Before closing[m
[32m+[m			[32mafterClose   : $.noop  // After closing[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m//Current state[m
[32m+[m		[32mgroup    : {}, // Selected group[m
[32m+[m		[32mopts     : {}, // Group options[m
[32m+[m		[32mprevious : null,  // Previous element[m
[32m+[m		[32mcoming   : null,  // Element being loaded[m
[32m+[m		[32mcurrent  : null,  // Currently loaded element[m
[32m+[m		[32misActive : false, // Is activated[m
[32m+[m		[32misOpen   : false, // Is currently open[m
[32m+[m		[32misOpened : false, // Have been fully opened at least once[m
[32m+[m
[32m+[m		[32mwrap  : null,[m
[32m+[m		[32mskin  : null,[m
[32m+[m		[32mouter : null,[m
[32m+[m		[32minner : null,[m
[32m+[m
[32m+[m		[32mplayer : {[m
[32m+[m			[32mtimer    : null,[m
[32m+[m			[32misActive : false[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Loaders[m
[32m+[m		[32majaxLoad   : null,[m
[32m+[m		[32mimgPreload : null,[m
[32m+[m
[32m+[m		[32m// Some collections[m
[32m+[m		[32mtransitions : {},[m
[32m+[m		[32mhelpers     : {},[m
[32m+[m
[32m+[m		[32m/*[m
[32m+[m		[32m *	Static methods[m
[32m+[m		[32m */[m
[32m+[m
[32m+[m		[32mopen: function (group, opts) {[m
[32m+[m			[32mif (!group) {[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (!$.isPlainObject(opts)) {[m
[32m+[m				[32mopts = {};[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Close if already active[m
[32m+[m			[32mif (false === F.close(true)) {[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Normalize group[m
[32m+[m			[32mif (!$.isArray(group)) {[m
[32m+[m				[32mgroup = isQuery(group) ? $(group).get() : [group];[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Recheck if the type of each element is `object` and set content type (image, ajax, etc)[m
[32m+[m			[32m$.each(group, function(i, element) {[m
[32m+[m				[32mvar obj = {},[m
[32m+[m					[32mhref,[m
[32m+[m					[32mtitle,[m
[32m+[m					[32mcontent,[m
[32m+[m					[32mtype,[m
[32m+[m					[32mrez,[m
[32m+[m					[32mhrefParts,[m
[32m+[m					[32mselector;[m
[32m+[m
[32m+[m				[32mif ($.type(element) === "object") {[m
[32m+[m					[32m// Check if is DOM element[m
[32m+[m					[32mif (element.nodeType) {[m
[32m+[m						[32melement = $(element);[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32mif (isQuery(element)) {[m
[32m+[m						[32mobj = {[m
[32m+[m							[32mhref    : element.data('fancybox-href') || element.attr('href'),[m
[32m+[m							[32mtitle   : element.data('fancybox-title') || element.attr('title'),[m
[32m+[m							[32misDom   : true,[m
[32m+[m							[32melement : element[m
[32m+[m						[32m};[m
[32m+[m
[32m+[m						[32mif ($.metadata) {[m
[32m+[m							[32m$.extend(true, obj, element.metadata());[m
[32m+[m						[32m}[m
[32m+[m
[32m+[m					[32m} else {[m
[32m+[m						[32mobj = element;[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mhref  = opts.href  || obj.href || (isString(element) ? element : null);[m
[32m+[m				[32mtitle = opts.title !== undefined ? opts.title : obj.title || '';[m
[32m+[m
[32m+[m				[32mcontent = opts.content || obj.content;[m
[32m+[m				[32mtype    = content ? 'html' : (opts.type  || obj.type);[m
[32m+[m
[32m+[m				[32mif (!type && obj.isDom) {[m
[32m+[m					[32mtype = element.data('fancybox-type');[m
[32m+[m
[32m+[m					[32mif (!type) {[m
[32m+[m						[32mrez  = element.prop('class').match(/fancybox\.(\w+)/);[m
[32m+[m						[32mtype = rez ? rez[1] : null;[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mif (isString(href)) {[m
[32m+[m					[32m// Try to guess the content type[m
[32m+[m					[32mif (!type) {[m
[32m+[m						[32mif (F.isImage(href)) {[m
[32m+[m							[32mtype = 'image';[m
[32m+[m
[32m+[m						[32m} else if (F.isSWF(href)) {[m
[32m+[m							[32mtype = 'swf';[m
[32m+[m
[32m+[m						[32m} else if (href.charAt(0) === '#') {[m
[32m+[m							[32mtype = 'inline';[m
[32m+[m
[32m+[m						[32m} else if (isString(element)) {[m
[32m+[m							[32mtype    = 'html';[m
[32m+[m							[32mcontent = element;[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32m// Split url into two pieces with source url and content selector, e.g,[m
[32m+[m					[32m// "/mypage.html #my_id" will load "/mypage.html" and display element having id "my_id"[m
[32m+[m					[32mif (type === 'ajax') {[m
[32m+[m						[32mhrefParts = href.split(/\s+/, 2);[m
[32m+[m						[32mhref      = hrefParts.shift();[m
[32m+[m						[32mselector  = hrefParts.shift();[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mif (!content) {[m
[32m+[m					[32mif (type === 'inline') {[m
[32m+[m						[32mif (href) {[m
[32m+[m							[32mcontent = $( isString(href) ? href.replace(/.*(?=#[^\s]+$)/, '') : href ); //strip for ie7[m
[32m+[m
[32m+[m						[32m} else if (obj.isDom) {[m
[32m+[m							[32mcontent = element;[m
[32m+[m						[32m}[m
[32m+[m
[32m+[m					[32m} else if (type === 'html') {[m
[32m+[m						[32mcontent = href;[m
[32m+[m
[32m+[m					[32m} else if (!type && !href && obj.isDom) {[m
[32m+[m						[32mtype    = 'inline';[m
[32m+[m						[32mcontent = element;[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m$.extend(obj, {[m
[32m+[m					[32mhref     : href,[m
[32m+[m					[32mtype     : type,[m
[32m+[m					[32mcontent  : content,[m
[32m+[m					[32mtitle    : title,[m
[32m+[m					[32mselector : selector[m
[32m+[m				[32m});[m
[32m+[m
[32m+[m				[32mgroup[ i ] = obj;[m
[32m+[m			[32m});[m
[32m+[m
[32m+[m			[32m// Extend the defaults[m
[32m+[m			[32mF.opts = $.extend(true, {}, F.defaults, opts);[m
[32m+[m
[32m+[m			[32m// All options are merged recursive except keys[m
[32m+[m			[32mif (opts.keys !== undefined) {[m
[32m+[m				[32mF.opts.keys = opts.keys ? $.extend({}, F.defaults.keys, opts.keys) : false;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mF.group = group;[m
[32m+[m
[32m+[m			[32mreturn F._start(F.opts.index);[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Cancel image loading or abort ajax request[m
[32m+[m		[32mcancel: function () {[m
[32m+[m			[32mvar coming = F.coming;[m
[32m+[m
[32m+[m			[32mif (!coming || false === F.trigger('onCancel')) {[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mF.hideLoading();[m
[32m+[m
[32m+[m			[32mif (F.ajaxLoad) {[m
[32m+[m				[32mF.ajaxLoad.abort();[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mF.ajaxLoad = null;[m
[32m+[m
[32m+[m			[32mif (F.imgPreload) {[m
[32m+[m				[32mF.imgPreload.onload = F.imgPreload.onerror = null;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (coming.wrap) {[m
[32m+[m				[32mcoming.wrap.stop(true, true).trigger('onReset').remove();[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mF.coming = null;[m
[32m+[m
[32m+[m			[32m// If the first item has been canceled, then clear everything[m
[32m+[m			[32mif (!F.current) {[m
[32m+[m				[32mF._afterZoomOut( coming );[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Start closing animation if is open; remove immediately if opening/closing[m
[32m+[m		[32mclose: function (event) {[m
[32m+[m			[32mF.cancel();[m
[32m+[m
[32m+[m			[32mif (false === F.trigger('beforeClose')) {[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mF.unbindEvents();[m
[32m+[m
[32m+[m			[32mif (!F.isActive) {[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (!F.isOpen || event === true) {[m
[32m+[m				[32m$('.fancybox-wrap').stop(true).trigger('onReset').remove();[m
[32m+[m
[32m+[m				[32mF._afterZoomOut();[m
[32m+[m
[32m+[m			[32m} else {[m
[32m+[m				[32mF.isOpen = F.isOpened = false;[m
[32m+[m				[32mF.isClosing = true;[m
[32m+[m
[32m+[m				[32m$('.fancybox-item, .fancybox-nav').remove();[m
[32m+[m
[32m+[m				[32mF.wrap.stop(true, true).removeClass('fancybox-opened');[m
[32m+[m
[32m+[m				[32mF.transitions[ F.current.closeMethod ]();[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Manage slideshow:[m
[32m+[m		[32m//   $.fancybox.play(); - toggle slideshow[m
[32m+[m		[32m//   $.fancybox.play( true ); - start[m
[32m+[m		[32m//   $.fancybox.play( false ); - stop[m
[32m+[m		[32mplay: function ( action ) {[m
[32m+[m			[32mvar clear = function () {[m
[32m+[m					[32mclearTimeout(F.player.timer);[m
[32m+[m				[32m},[m
[32m+[m				[32mset = function () {[m
[32m+[m					[32mclear();[m
[32m+[m
[32m+[m					[32mif (F.current && F.player.isActive) {[m
[32m+[m						[32mF.player.timer = setTimeout(F.next, F.current.playSpeed);[m
[32m+[m					[32m}[m
[32m+[m				[32m},[m
[32m+[m				[32mstop = function () {[m
[32m+[m					[32mclear();[m
[32m+[m
[32m+[m					[32m$('body').unbind('.player');[m
[32m+[m
[32m+[m					[32mF.player.isActive = false;[m
[32m+[m
[32m+[m					[32mF.trigger('onPlayEnd');[m
[32m+[m				[32m},[m
[32m+[m				[32mstart = function () {[m
[32m+[m					[32mif (F.current && (F.current.loop || F.current.index < F.group.length - 1)) {[m
[32m+[m						[32mF.player.isActive = true;[m
[32m+[m
[32m+[m						[32m$('body').bind({[m
[32m+[m							[32m'afterShow.player onUpdate.player'   : set,[m
[32m+[m							[32m'onCancel.player beforeClose.player' : stop,[m
[32m+[m							[32m'beforeLoad.player' : clear[m
[32m+[m						[32m});[m
[32m+[m
[32m+[m						[32mset();[m
[32m+[m
[32m+[m						[32mF.trigger('onPlayStart');[m
[32m+[m					[32m}[m
[32m+[m				[32m};[m
[32m+[m
[32m+[m			[32mif (action === true || (!F.player.isActive && action !== false)) {[m
[32m+[m				[32mstart();[m
[32m+[m			[32m} else {[m
[32m+[m				[32mstop();[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Navigate to next gallery item[m
[32m+[m		[32mnext: function ( direction ) {[m
[32m+[m			[32mvar current = F.current;[m
[32m+[m
[32m+[m			[32mif (current) {[m
[32m+[m				[32mif (!isString(direction)) {[m
[32m+[m					[32mdirection = current.direction.next;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mF.jumpto(current.index + 1, direction, 'next');[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Navigate to previous gallery item[m
[32m+[m		[32mprev: function ( direction ) {[m
[32m+[m			[32mvar current = F.current;[m
[32m+[m
[32m+[m			[32mif (current) {[m
[32m+[m				[32mif (!isString(direction)) {[m
[32m+[m					[32mdirection = current.direction.prev;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mF.jumpto(current.index - 1, direction, 'prev');[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Navigate to gallery item by index[m
[32m+[m		[32mjumpto: function ( index, direction, router ) {[m
[32m+[m			[32mvar current = F.current;[m
[32m+[m
[32m+[m			[32mif (!current) {[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mindex = getScalar(index);[m
[32m+[m
[32m+[m			[32mF.direction = direction || current.direction[ (index >= current.index ? 'next' : 'prev') ];[m
[32m+[m			[32mF.router    = router || 'jumpto';[m
[32m+[m
[32m+[m			[32mif (current.loop) {[m
[32m+[m				[32mif (index < 0) {[m
[32m+[m					[32mindex = current.group.length + (index % current.group.length);[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mindex = index % current.group.length;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (current.group[ index ] !== undefined) {[m
[32m+[m				[32mF.cancel();[m
[32m+[m
[32m+[m				[32mF._start(index);[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Center inside viewport and toggle position type to fixed or absolute if needed[m
[32m+[m		[32mreposition: function (e, onlyAbsolute) {[m
[32m+[m			[32mvar current = F.current,[m
[32m+[m				[32mwrap    = current ? current.wrap : null,[m
[32m+[m				[32mpos;[m
[32m+[m
[32m+[m			[32mif (wrap) {[m
[32m+[m				[32mpos = F._getPosition(onlyAbsolute);[m
[32m+[m
[32m+[m				[32mif (e && e.type === 'scroll') {[m
[32m+[m					[32mdelete pos.position;[m
[32m+[m
[32m+[m					[32mwrap.stop(true, true).animate(pos, 200);[m
[32m+[m
[32m+[m				[32m} else {[m
[32m+[m					[32mwrap.css(pos);[m
[32m+[m
[32m+[m					[32mcurrent.pos = $.extend({}, current.dim, pos);[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32mupdate: function (e) {[m
[32m+[m			[32mvar type = (e && e.type),[m
[32m+[m				[32manyway = !type || type === 'orientationchange';[m
[32m+[m
[32m+[m			[32mif (anyway) {[m
[32m+[m				[32mclearTimeout(didUpdate);[m
[32m+[m
[32m+[m				[32mdidUpdate = null;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (!F.isOpen || didUpdate) {[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mdidUpdate = setTimeout(function() {[m
[32m+[m				[32mvar current = F.current;[m
[32m+[m
[32m+[m				[32mif (!current || F.isClosing) {[m
[32m+[m					[32mreturn;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mF.wrap.removeClass('fancybox-tmp');[m
[32m+[m
[32m+[m				[32mif (anyway || type === 'load' || (type === 'resize' && current.autoResize)) {[m
[32m+[m					[32mF._setDimension();[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mif (!(type === 'scroll' && current.canShrink)) {[m
[32m+[m					[32mF.reposition(e);[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mF.trigger('onUpdate');[m
[32m+[m
[32m+[m				[32mdidUpdate = null;[m
[32m+[m
[32m+[m			[32m}, (anyway && !isTouch ? 0 : 300));[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Shrink content to fit inside viewport or restore if resized[m
[32m+[m		[32mtoggle: function ( action ) {[m
[32m+[m			[32mif (F.isOpen) {[m
[32m+[m				[32mF.current.fitToView = $.type(action) === "boolean" ? action : !F.current.fitToView;[m
[32m+[m
[32m+[m				[32m// Help browser to restore document dimensions[m
[32m+[m				[32mif (isTouch) {[m
[32m+[m					[32mF.wrap.removeAttr('style').addClass('fancybox-tmp');[m
[32m+[m
[32m+[m					[32mF.trigger('onUpdate');[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mF.update();[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32mhideLoading: function () {[m
[32m+[m			[32mD.unbind('.loading');[m
[32m+[m
[32m+[m			[32m$('#fancybox-loading').remove();[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32mshowLoading: function () {[m
[32m+[m			[32mvar el, viewport;[m
[32m+[m
[32m+[m			[32mF.hideLoading();[m
[32m+[m
[32m+[m			[32mel = $('<div id="fancybox-loading"><div></div></div>').click(F.cancel).appendTo('body');[m
[32m+[m
[32m+[m			[32m// If user will press the escape-button, the request will be canceled[m
[32m+[m			[32mD.bind('keydown.loading', function(e) {[m
[32m+[m				[32mif ((e.which || e.keyCode) === 27) {[m
[32m+[m					[32me.preventDefault();[m
[32m+[m
[32m+[m					[32mF.cancel();[m
[32m+[m				[32m}[m
[32m+[m			[32m});[m
[32m+[m
[32m+[m			[32mif (!F.defaults.fixed) {[m
[32m+[m				[32mviewport = F.getViewport();[m
[32m+[m
[32m+[m				[32mel.css({[m
[32m+[m					[32mposition : 'absolute',[m
[32m+[m					[32mtop  : (viewport.h * 0.5) + viewport.y,[m
[32m+[m					[32mleft : (viewport.w * 0.5) + viewport.x[m
[32m+[m				[32m});[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32mgetViewport: function () {[m
[32m+[m			[32mvar locked = (F.current && F.current.locked) || false,[m
[32m+[m				[32mrez    = {[m
[32m+[m					[32mx: W.scrollLeft(),[m
[32m+[m					[32my: W.scrollTop()[m
[32m+[m				[32m};[m
[32m+[m
[32m+[m			[32mif (locked) {[m
[32m+[m				[32mrez.w = locked[0].clientWidth;[m
[32m+[m				[32mrez.h = locked[0].clientHeight;[m
[32m+[m
[32m+[m			[32m} else {[m
[32m+[m				[32m// See http://bugs.jquery.com/ticket/6724[m
[32m+[m				[32mrez.w = isTouch && window.innerWidth  ? window.innerWidth  : W.width();[m
[32m+[m				[32mrez.h = isTouch && window.innerHeight ? window.innerHeight : W.height();[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mreturn rez;[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Unbind the keyboard / clicking actions[m
[32m+[m		[32munbindEvents: function () {[m
[32m+[m			[32mif (F.wrap && isQuery(F.wrap)) {[m
[32m+[m				[32mF.wrap.unbind('.fb');[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mD.unbind('.fb');[m
[32m+[m			[32mW.unbind('.fb');[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32mbindEvents: function () {[m
[32m+[m			[32mvar current = F.current,[m
[32m+[m				[32mkeys;[m
[32m+[m
[32m+[m			[32mif (!current) {[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Changing document height on iOS devices triggers a 'resize' event,[m
[32m+[m			[32m// that can change document height... repeating infinitely[m
[32m+[m			[32mW.bind('orientationchange.fb' + (isTouch ? '' : ' resize.fb') + (current.autoCenter && !current.locked ? ' scroll.fb' : ''), F.update);[m
[32m+[m
[32m+[m			[32mkeys = current.keys;[m
[32m+[m
[32m+[m			[32mif (keys) {[m
[32m+[m				[32mD.bind('keydown.fb', function (e) {[m
[32m+[m					[32mvar code   = e.which || e.keyCode,[m
[32m+[m						[32mtarget = e.target || e.srcElement;[m
[32m+[m
[32m+[m					[32m// Skip esc key if loading, because showLoading will cancel preloading[m
[32m+[m					[32mif (code === 27 && F.coming) {[m
[32m+[m						[32mreturn false;[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32m// Ignore key combinations and key events within form elements[m
[32m+[m					[32mif (!e.ctrlKey && !e.altKey && !e.shiftKey && !e.metaKey && !(target && (target.type || $(target).is('[contenteditable]')))) {[m
[32m+[m						[32m$.each(keys, function(i, val) {[m
[32m+[m							[32mif (current.group.length > 1 && val[ code ] !== undefined) {[m
[32m+[m								[32mF[ i ]( val[ code ] );[m
[32m+[m
[32m+[m								[32me.preventDefault();[m
[32m+[m								[32mreturn false;[m
[32m+[m							[32m}[m
[32m+[m
[32m+[m							[32mif ($.inArray(code, val) > -1) {[m
[32m+[m								[32mF[ i ] ();[m
[32m+[m
[32m+[m								[32me.preventDefault();[m
[32m+[m								[32mreturn false;[m
[32m+[m							[32m}[m
[32m+[m						[32m});[m
[32m+[m					[32m}[m
[32m+[m				[32m});[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif ($.fn.mousewheel && current.mouseWheel) {[m
[32m+[m				[32mF.wrap.bind('mousewheel.fb', function (e, delta, deltaX, deltaY) {[m
[32m+[m					[32mvar target = e.target || null,[m
[32m+[m						[32mparent = $(target),[m
[32m+[m						[32mcanScroll = false;[m
[32m+[m
[32m+[m					[32mwhile (parent.length) {[m
[32m+[m						[32mif (canScroll || parent.is('.fancybox-skin') || parent.is('.fancybox-wrap')) {[m
[32m+[m							[32mbreak;[m
[32m+[m						[32m}[m
[32m+[m
[32m+[m						[32mcanScroll = isScrollable( parent[0] );[m
[32m+[m						[32mparent    = $(parent).parent();[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32mif (delta !== 0 && !canScroll) {[m
[32m+[m						[32mif (F.group.length > 1 && !current.canShrink) {[m
[32m+[m							[32mif (deltaY > 0 || deltaX > 0) {[m
[32m+[m								[32mF.prev( deltaY > 0 ? 'down' : 'left' );[m
[32m+[m
[32m+[m							[32m} else if (deltaY < 0 || deltaX < 0) {[m
[32m+[m								[32mF.next( deltaY < 0 ? 'up' : 'right' );[m
[32m+[m							[32m}[m
[32m+[m
[32m+[m							[32me.preventDefault();[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m				[32m});[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32mtrigger: function (event, o) {[m
[32m+[m			[32mvar ret, obj = o || F.coming || F.current;[m
[32m+[m
[32m+[m			[32mif (!obj) {[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif ($.isFunction( obj[event] )) {[m
[32m+[m				[32mret = obj[event].apply(obj, Array.prototype.slice.call(arguments, 1));[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (ret === false) {[m
[32m+[m				[32mreturn false;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (obj.helpers) {[m
[32m+[m				[32m$.each(obj.helpers, function (helper, opts) {[m
[32m+[m					[32mif (opts && F.helpers[helper] && $.isFunction(F.helpers[helper][event])) {[m
[32m+[m						[32mopts = $.extend(true, {}, F.helpers[helper].defaults, opts);[m
[32m+[m
[32m+[m						[32mF.helpers[helper][event](opts, obj);[m
[32m+[m					[32m}[m
[32m+[m				[32m});[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m$.event.trigger(event + '.fb');[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32misImage: function (str) {[m
[32m+[m			[32mreturn isString(str) && str.match(/(^data:image\/.*,)|(\.(jp(e|g|eg)|gif|png|bmp|webp)((\?|#).*)?$)/i);[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32misSWF: function (str) {[m
[32m+[m			[32mreturn isString(str) && str.match(/\.(swf)((\?|#).*)?$/i);[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m_start: function (index) {[m
[32m+[m			[32mvar coming = {},[m
[32m+[m				[32mobj,[m
[32m+[m				[32mhref,[m
[32m+[m				[32mtype,[m
[32m+[m				[32mmargin,[m
[32m+[m				[32mpadding;[m
[32m+[m
[32m+[m			[32mindex = getScalar( index );[m
[32m+[m			[32mobj   = F.group[ index ] || null;[m
[32m+[m
[32m+[m			[32mif (!obj) {[m
[32m+[m				[32mreturn false;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mcoming = $.extend(true, {}, F.opts, obj);[m
[32m+[m
[32m+[m			[32m// Convert margin and padding properties to array - top, right, bottom, left[m
[32m+[m			[32mmargin  = coming.margin;[m
[32m+[m			[32mpadding = coming.padding;[m
[32m+[m
[32m+[m			[32mif ($.type(margin) === 'number') {[m
[32m+[m				[32mcoming.margin = [margin, margin, margin, margin];[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif ($.type(padding) === 'number') {[m
[32m+[m				[32mcoming.padding = [padding, padding, padding, padding];[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// 'modal' propery is just a shortcut[m
[32m+[m			[32mif (coming.modal) {[m
[32m+[m				[32m$.extend(true, coming, {[m
[32m+[m					[32mcloseBtn   : false,[m
[32m+[m					[32mcloseClick : false,[m
[32m+[m					[32mnextClick  : false,[m
[32m+[m					[32marrows     : false,[m
[32m+[m					[32mmouseWheel : false,[m
[32m+[m					[32mkeys       : null,[m
[32m+[m					[32mhelpers: {[m
[32m+[m						[32moverlay : {[m
[32m+[m							[32mcloseClick : false[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m				[32m});[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// 'autoSize' property is a shortcut, too[m
[32m+[m			[32mif (coming.autoSize) {[m
[32m+[m				[32mcoming.autoWidth = coming.autoHeight = true;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (coming.width === 'auto') {[m
[32m+[m				[32mcoming.autoWidth = true;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (coming.height === 'auto') {[m
[32m+[m				[32mcoming.autoHeight = true;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m/*[m
[32m+[m			[32m * Add reference to the group, so it`s possible to access from callbacks, example:[m
[32m+[m			[32m * afterLoad : function() {[m
[32m+[m			[32m *     this.title = 'Image ' + (this.index + 1) + ' of ' + this.group.length + (this.title ? ' - ' + this.title : '');[m
[32m+[m			[32m * }[m
[32m+[m			[32m */[m
[32m+[m
[32m+[m			[32mcoming.group  = F.group;[m
[32m+[m			[32mcoming.index  = index;[m
[32m+[m
[32m+[m			[32m// Give a chance for callback or helpers to update coming item (type, title, etc)[m
[32m+[m			[32mF.coming = coming;[m
[32m+[m
[32m+[m			[32mif (false === F.trigger('beforeLoad')) {[m
[32m+[m				[32mF.coming = null;[m
[32m+[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mtype = coming.type;[m
[32m+[m			[32mhref = coming.href;[m
[32m+[m
[32m+[m			[32mif (!type) {[m
[32m+[m				[32mF.coming = null;[m
[32m+[m
[32m+[m				[32m//If we can not determine content type then drop silently or display next/prev item if looping through gallery[m
[32m+[m				[32mif (F.current && F.router && F.router !== 'jumpto') {[m
[32m+[m					[32mF.current.index = index;[m
[32m+[m
[32m+[m					[32mreturn F[ F.router ]( F.direction );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mreturn false;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mF.isActive = true;[m
[32m+[m
[32m+[m			[32mif (type === 'image' || type === 'swf') {[m
[32m+[m				[32mcoming.autoHeight = coming.autoWidth = false;[m
[32m+[m				[32mcoming.scrolling  = 'visible';[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (type === 'image') {[m
[32m+[m				[32mcoming.aspectRatio = true;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (type === 'iframe' && isTouch) {[m
[32m+[m				[32mcoming.scrolling = 'scroll';[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Build the neccessary markup[m
[32m+[m			[32mcoming.wrap = $(coming.tpl.wrap).addClass('fancybox-' + (isTouch ? 'mobile' : 'desktop') + ' fancybox-type-' + type + ' fancybox-tmp ' + coming.wrapCSS).appendTo( coming.parent || 'body' );[m
[32m+[m
[32m+[m			[32m$.extend(coming, {[m
[32m+[m				[32mskin  : $('.fancybox-skin',  coming.wrap),[m
[32m+[m				[32mouter : $('.fancybox-outer', coming.wrap),[m
[32m+[m				[32minner : $('.fancybox-inner', coming.wrap)[m
[32m+[m			[32m});[m
[32m+[m
[32m+[m			[32m$.each(["Top", "Right", "Bottom", "Left"], function(i, v) {[m
[32m+[m				[32mcoming.skin.css('padding' + v, getValue(coming.padding[ i ]));[m
[32m+[m			[32m});[m
[32m+[m
[32m+[m			[32mF.trigger('onReady');[m
[32m+[m
[32m+[m			[32m// Check before try to load; 'inline' and 'html' types need content, others - href[m
[32m+[m			[32mif (type === 'inline' || type === 'html') {[m
[32m+[m				[32mif (!coming.content || !coming.content.length) {[m
[32m+[m					[32mreturn F._error( 'content' );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m			[32m} else if (!href) {[m
[32m+[m				[32mreturn F._error( 'href' );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (type === 'image') {[m
[32m+[m				[32mF._loadImage();[m
[32m+[m
[32m+[m			[32m} else if (type === 'ajax') {[m
[32m+[m				[32mF._loadAjax();[m
[32m+[m
[32m+[m			[32m} else if (type === 'iframe') {[m
[32m+[m				[32mF._loadIframe();[m
[32m+[m
[32m+[m			[32m} else {[m
[32m+[m				[32mF._afterLoad();[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m_error: function ( type ) {[m
[32m+[m			[32m$.extend(F.coming, {[m
[32m+[m				[32mtype       : 'html',[m
[32m+[m				[32mautoWidth  : true,[m
[32m+[m				[32mautoHeight : true,[m
[32m+[m				[32mminWidth   : 0,[m
[32m+[m				[32mminHeight  : 0,[m
[32m+[m				[32mscrolling  : 'no',[m
[32m+[m				[32mhasError   : type,[m
[32m+[m				[32mcontent    : F.coming.tpl.error[m
[32m+[m			[32m});[m
[32m+[m
[32m+[m			[32mF._afterLoad();[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m_loadImage: function () {[m
[32m+[m			[32m// Reset preload image so it is later possible to check "complete" property[m
[32m+[m			[32mvar img = F.imgPreload = new Image();[m
[32m+[m
[32m+[m			[32mimg.onload = function () {[m
[32m+[m				[32mthis.onload = this.onerror = null;[m
[32m+[m
[32m+[m				[32mF.coming.width  = this.width;[m
[32m+[m				[32mF.coming.height = this.height;[m
[32m+[m
[32m+[m				[32mF._afterLoad();[m
[32m+[m			[32m};[m
[32m+[m
[32m+[m			[32mimg.onerror = function () {[m
[32m+[m				[32mthis.onload = this.onerror = null;[m
[32m+[m
[32m+[m				[32mF._error( 'image' );[m
[32m+[m			[32m};[m
[32m+[m
[32m+[m			[32mimg.src = F.coming.href;[m
[32m+[m
[32m+[m			[32mif (img.complete !== true) {[m
[32m+[m				[32mF.showLoading();[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m_loadAjax: function () {[m
[32m+[m			[32mvar coming = F.coming;[m
[32m+[m
[32m+[m			[32mF.showLoading();[m
[32m+[m
[32m+[m			[32mF.ajaxLoad = $.ajax($.extend({}, coming.ajax, {[m
[32m+[m				[32murl: coming.href,[m
[32m+[m				[32merror: function (jqXHR, textStatus) {[m
[32m+[m					[32mif (F.coming && textStatus !== 'abort') {[m
[32m+[m						[32mF._error( 'ajax', jqXHR );[m
[32m+[m
[32m+[m					[32m} else {[m
[32m+[m						[32mF.hideLoading();[m
[32m+[m					[32m}[m
[32m+[m				[32m},[m
[32m+[m				[32msuccess: function (data, textStatus) {[m
[32m+[m					[32mif (textStatus === 'success') {[m
[32m+[m						[32mcoming.content = data;[m
[32m+[m
[32m+[m						[32mF._afterLoad();[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}));[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m_loadIframe: function() {[m
[32m+[m			[32mvar coming = F.coming,[m
[32m+[m				[32miframe = $(coming.tpl.iframe.replace(/\{rnd\}/g, new Date().getTime()))[m
[32m+[m					[32m.attr('scrolling', isTouch ? 'auto' : coming.iframe.scrolling)[m
[32m+[m					[32m.attr('src', coming.href);[m
[32m+[m
[32m+[m			[32m// This helps IE[m
[32m+[m			[32m$(coming.wrap).bind('onReset', function () {[m
[32m+[m				[32mtry {[m
[32m+[m					[32m$(this).find('iframe').hide().attr('src', '//about:blank').end().empty();[m
[32m+[m				[32m} catch (e) {}[m
[32m+[m			[32m});[m
[32m+[m
[32m+[m			[32mif (coming.iframe.preload) {[m
[32m+[m				[32mF.showLoading();[m
[32m+[m
[32m+[m				[32miframe.one('load', function() {[m
[32m+[m					[32m$(this).data('ready', 1);[m
[32m+[m
[32m+[m					[32m// iOS will lose scrolling if we resize[m
[32m+[m					[32mif (!isTouch) {[m
[32m+[m						[32m$(this).bind('load.fb', F.update);[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32m// Without this trick:[m
[32m+[m					[32m//   - iframe won't scroll on iOS devices[m
[32m+[m					[32m//   - IE7 sometimes displays empty iframe[m
[32m+[m					[32m$(this).parents('.fancybox-wrap').width('100%').removeClass('fancybox-tmp').show();[m
[32m+[m
[32m+[m					[32mF._afterLoad();[m
[32m+[m				[32m});[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mcoming.content = iframe.appendTo( coming.inner );[m
[32m+[m
[32m+[m			[32mif (!coming.iframe.preload) {[m
[32m+[m				[32mF._afterLoad();[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m_preloadImages: function() {[m
[32m+[m			[32mvar group   = F.group,[m
[32m+[m				[32mcurrent = F.current,[m
[32m+[m				[32mlen     = group.length,[m
[32m+[m				[32mcnt     = current.preload ? Math.min(current.preload, len - 1) : 0,[m
[32m+[m				[32mitem,[m
[32m+[m				[32mi;[m
[32m+[m
[32m+[m			[32mfor (i = 1; i <= cnt; i += 1) {[m
[32m+[m				[32mitem = group[ (current.index + i ) % len ];[m
[32m+[m
[32m+[m				[32mif (item.type === 'image' && item.href) {[m
[32m+[m					[32mnew Image().src = item.href;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m_afterLoad: function () {[m
[32m+[m			[32mvar coming   = F.coming,[m
[32m+[m				[32mprevious = F.current,[m
[32m+[m				[32mplaceholder = 'fancybox-placeholder',[m
[32m+[m				[32mcurrent,[m
[32m+[m				[32mcontent,[m
[32m+[m				[32mtype,[m
[32m+[m				[32mscrolling,[m
[32m+[m				[32mhref,[m
[32m+[m				[32membed;[m
[32m+[m
[32m+[m			[32mF.hideLoading();[m
[32m+[m
[32m+[m			[32mif (!coming || F.isActive === false) {[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (false === F.trigger('afterLoad', coming, previous)) {[m
[32m+[m				[32mcoming.wrap.stop(true).trigger('onReset').remove();[m
[32m+[m
[32m+[m				[32mF.coming = null;[m
[32m+[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (previous) {[m
[32m+[m				[32mF.trigger('beforeChange', previous);[m
[32m+[m
[32m+[m				[32mprevious.wrap.stop(true).removeClass('fancybox-opened')[m
[32m+[m					[32m.find('.fancybox-item, .fancybox-nav')[m
[32m+[m					[32m.remove();[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mF.unbindEvents();[m
[32m+[m
[32m+[m			[32mcurrent   = coming;[m
[32m+[m			[32mcontent   = coming.content;[m
[32m+[m			[32mtype      = coming.type;[m
[32m+[m			[32mscrolling = coming.scrolling;[m
[32m+[m
[32m+[m			[32m$.extend(F, {[m
[32m+[m				[32mwrap  : current.wrap,[m
[32m+[m				[32mskin  : current.skin,[m
[32m+[m				[32mouter : current.outer,[m
[32m+[m				[32minner : current.inner,[m
[32m+[m				[32mcurrent  : current,[m
[32m+[m				[32mprevious : previous[m
[32m+[m			[32m});[m
[32m+[m
[32m+[m			[32mhref = current.href;[m
[32m+[m
[32m+[m			[32mswitch (type) {[m
[32m+[m				[32mcase 'inline':[m
[32m+[m				[32mcase 'ajax':[m
[32m+[m				[32mcase 'html':[m
[32m+[m					[32mif (current.selector) {[m
[32m+[m						[32mcontent = $('<div>').html(content).find(current.selector);[m
[32m+[m
[32m+[m					[32m} else if (isQuery(content)) {[m
[32m+[m						[32mif (!content.data(placeholder)) {[m
[32m+[m							[32mcontent.data(placeholder, $('<div class="' + placeholder + '"></div>').insertAfter( content ).hide() );[m
[32m+[m						[32m}[m
[32m+[m
[32m+[m						[32mcontent = content.show().detach();[m
[32m+[m
[32m+[m						[32mcurrent.wrap.bind('onReset', function () {[m
[32m+[m							[32mif ($(this).find(content).length) {[m
[32m+[m								[32mcontent.hide().replaceAll( content.data(placeholder) ).data(placeholder, false);[m
[32m+[m							[32m}[m
[32m+[m						[32m});[m
[32m+[m					[32m}[m
[32m+[m				[32mbreak;[m
[32m+[m
[32m+[m				[32mcase 'image':[m
[32m+[m					[32mcontent = current.tpl.image.replace('{href}', href);[m
[32m+[m				[32mbreak;[m
[32m+[m
[32m+[m				[32mcase 'swf':[m
[32m+[m					[32mcontent = '<object id="fancybox-swf" classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" width="100%" height="100%"><param name="movie" value="' + href + '"></param>';[m
[32m+[m					[32membed   = '';[m
[32m+[m
[32m+[m					[32m$.each(current.swf, function(name, val) {[m
[32m+[m						[32mcontent += '<param name="' + name + '" value="' + val + '"></param>';[m
[32m+[m						[32membed   += ' ' + name + '="' + val + '"';[m
[32m+[m					[32m});[m
[32m+[m
[32m+[m					[32mcontent += '<embed src="' + href + '" type="application/x-shockwave-flash" width="100%" height="100%"' + embed + '></embed></object>';[m
[32m+[m				[32mbreak;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (!(isQuery(content) && content.parent().is(current.inner))) {[m
[32m+[m				[32mcurrent.inner.append( content );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Give a chance for helpers or callbacks to update elements[m
[32m+[m			[32mF.trigger('beforeShow');[m
[32m+[m
[32m+[m			[32m// Set scrolling before calculating dimensions[m
[32m+[m			[32mcurrent.inner.css('overflow', scrolling === 'yes' ? 'scroll' : (scrolling === 'no' ? 'hidden' : scrolling));[m
[32m+[m
[32m+[m			[32m// Set initial dimensions and start position[m
[32m+[m			[32mF._setDimension();[m
[32m+[m
[32m+[m			[32mF.reposition();[m
[32m+[m
[32m+[m			[32mF.isOpen = false;[m
[32m+[m			[32mF.coming = null;[m
[32m+[m
[32m+[m			[32mF.bindEvents();[m
[32m+[m
[32m+[m			[32mif (!F.isOpened) {[m
[32m+[m				[32m$('.fancybox-wrap').not( current.wrap ).stop(true).trigger('onReset').remove();[m
[32m+[m
[32m+[m			[32m} else if (previous.prevMethod) {[m
[32m+[m				[32mF.transitions[ previous.prevMethod ]();[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mF.transitions[ F.isOpened ? current.nextMethod : current.openMethod ]();[m
[32m+[m
[32m+[m			[32mF._preloadImages();[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m_setDimension: function () {[m
[32m+[m			[32mvar viewport   = F.getViewport(),[m
[32m+[m				[32msteps      = 0,[m
[32m+[m				[32mcanShrink  = false,[m
[32m+[m				[32mcanExpand  = false,[m
[32m+[m				[32mwrap       = F.wrap,[m
[32m+[m				[32mskin       = F.skin,[m
[32m+[m				[32minner      = F.inner,[m
[32m+[m				[32mcurrent    = F.current,[m
[32m+[m				[32mwidth      = current.width,[m
[32m+[m				[32mheight     = current.height,[m
[32m+[m				[32mminWidth   = current.minWidth,[m
[32m+[m				[32mminHeight  = current.minHeight,[m
[32m+[m				[32mmaxWidth   = current.maxWidth,[m
[32m+[m				[32mmaxHeight  = current.maxHeight,[m
[32m+[m				[32mscrolling  = current.scrolling,[m
[32m+[m				[32mscrollOut  = current.scrollOutside ? current.scrollbarWidth : 0,[m
[32m+[m				[32mmargin     = current.margin,[m
[32m+[m				[32mwMargin    = getScalar(margin[1] + margin[3]),[m
[32m+[m				[32mhMargin    = getScalar(margin[0] + margin[2]),[m
[32m+[m				[32mwPadding,[m
[32m+[m				[32mhPadding,[m
[32m+[m				[32mwSpace,[m
[32m+[m				[32mhSpace,[m
[32m+[m				[32morigWidth,[m
[32m+[m				[32morigHeight,[m
[32m+[m				[32morigMaxWidth,[m
[32m+[m				[32morigMaxHeight,[m
[32m+[m				[32mratio,[m
[32m+[m				[32mwidth_,[m
[32m+[m				[32mheight_,[m
[32m+[m				[32mmaxWidth_,[m
[32m+[m				[32mmaxHeight_,[m
[32m+[m				[32miframe,[m
[32m+[m				[32mbody;[m
[32m+[m
[32m+[m			[32m// Reset dimensions so we could re-check actual size[m
[32m+[m			[32mwrap.add(skin).add(inner).width('auto').height('auto').removeClass('fancybox-tmp');[m
[32m+[m
[32m+[m			[32mwPadding = getScalar(skin.outerWidth(true)  - skin.width());[m
[32m+[m			[32mhPadding = getScalar(skin.outerHeight(true) - skin.height());[m
[32m+[m
[32m+[m			[32m// Any space between content and viewport (margin, padding, border, title)[m
[32m+[m			[32mwSpace = wMargin + wPadding;[m
[32m+[m			[32mhSpace = hMargin + hPadding;[m
[32m+[m
[32m+[m			[32morigWidth  = isPercentage(width)  ? (viewport.w - wSpace) * getScalar(width)  / 100 : width;[m
[32m+[m			[32morigHeight = isPercentage(height) ? (viewport.h - hSpace) * getScalar(height) / 100 : height;[m
[32m+[m
[32m+[m			[32mif (current.type === 'iframe') {[m
[32m+[m				[32miframe = current.content;[m
[32m+[m
[32m+[m				[32mif (current.autoHeight && iframe.data('ready') === 1) {[m
[32m+[m					[32mtry {[m
[32m+[m						[32mif (iframe[0].contentWindow.document.location) {[m
[32m+[m							[32minner.width( origWidth ).height(9999);[m
[32m+[m
[32m+[m							[32mbody = iframe.contents().find('body');[m
[32m+[m
[32m+[m							[32mif (scrollOut) {[m
[32m+[m								[32mbody.css('overflow-x', 'hidden');[m
[32m+[m							[32m}[m
[32m+[m
[32m+[m							[32morigHeight = body.height();[m
[32m+[m						[32m}[m
[32m+[m
[32m+[m					[32m} catch (e) {}[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m			[32m} else if (current.autoWidth || current.autoHeight) {[m
[32m+[m				[32minner.addClass( 'fancybox-tmp' );[m
[32m+[m
[32m+[m				[32m// Set width or height in case we need to calculate only one dimension[m
[32m+[m				[32mif (!current.autoWidth) {[m
[32m+[m					[32minner.width( origWidth );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mif (!current.autoHeight) {[m
[32m+[m					[32minner.height( origHeight );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mif (current.autoWidth) {[m
[32m+[m					[32morigWidth = inner.width();[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mif (current.autoHeight) {[m
[32m+[m					[32morigHeight = inner.height();[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32minner.removeClass( 'fancybox-tmp' );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mwidth  = getScalar( origWidth );[m
[32m+[m			[32mheight = getScalar( origHeight );[m
[32m+[m
[32m+[m			[32mratio  = origWidth / origHeight;[m
[32m+[m
[32m+[m			[32m// Calculations for the content[m
[32m+[m			[32mminWidth  = getScalar(isPercentage(minWidth) ? getScalar(minWidth, 'w') - wSpace : minWidth);[m
[32m+[m			[32mmaxWidth  = getScalar(isPercentage(maxWidth) ? getScalar(maxWidth, 'w') - wSpace : maxWidth);[m
[32m+[m
[32m+[m			[32mminHeight = getScalar(isPercentage(minHeight) ? getScalar(minHeight, 'h') - hSpace : minHeight);[m
[32m+[m			[32mmaxHeight = getScalar(isPercentage(maxHeight) ? getScalar(maxHeight, 'h') - hSpace : maxHeight);[m
[32m+[m
[32m+[m			[32m// These will be used to determine if wrap can fit in the viewport[m
[32m+[m			[32morigMaxWidth  = maxWidth;[m
[32m+[m			[32morigMaxHeight = maxHeight;[m
[32m+[m
[32m+[m			[32mif (current.fitToView) {[m
[32m+[m				[32mmaxWidth  = Math.min(viewport.w - wSpace, maxWidth);[m
[32m+[m				[32mmaxHeight = Math.min(viewport.h - hSpace, maxHeight);[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mmaxWidth_  = viewport.w - wMargin;[m
[32m+[m			[32mmaxHeight_ = viewport.h - hMargin;[m
[32m+[m
[32m+[m			[32mif (current.aspectRatio) {[m
[32m+[m				[32mif (width > maxWidth) {[m
[32m+[m					[32mwidth  = maxWidth;[m
[32m+[m					[32mheight = getScalar(width / ratio);[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mif (height > maxHeight) {[m
[32m+[m					[32mheight = maxHeight;[m
[32m+[m					[32mwidth  = getScalar(height * ratio);[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mif (width < minWidth) {[m
[32m+[m					[32mwidth  = minWidth;[m
[32m+[m					[32mheight = getScalar(width / ratio);[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mif (height < minHeight) {[m
[32m+[m					[32mheight = minHeight;[m
[32m+[m					[32mwidth  = getScalar(height * ratio);[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m			[32m} else {[m
[32m+[m				[32mwidth = Math.max(minWidth, Math.min(width, maxWidth));[m
[32m+[m
[32m+[m				[32mif (current.autoHeight && current.type !== 'iframe') {[m
[32m+[m					[32minner.width( width );[m
[32m+[m
[32m+[m					[32mheight = inner.height();[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mheight = Math.max(minHeight, Math.min(height, maxHeight));[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Try to fit inside viewport (including the title)[m
[32m+[m			[32mif (current.fitToView) {[m
[32m+[m				[32minner.width( width ).height( height );[m
[32m+[m
[32m+[m				[32mwrap.width( width + wPadding );[m
[32m+[m
[32m+[m				[32m// Real wrap dimensions[m
[32m+[m				[32mwidth_  = wrap.width();[m
[32m+[m				[32mheight_ = wrap.height();[m
[32m+[m
[32m+[m				[32mif (current.aspectRatio) {[m
[32m+[m					[32mwhile ((width_ > maxWidth_ || height_ > maxHeight_) && width > minWidth && height > minHeight) {[m
[32m+[m						[32mif (steps++ > 19) {[m
[32m+[m							[32mbreak;[m
[32m+[m						[32m}[m
[32m+[m
[32m+[m						[32mheight = Math.max(minHeight, Math.min(maxHeight, height - 10));[m
[32m+[m						[32mwidth  = getScalar(height * ratio);[m
[32m+[m
[32m+[m						[32mif (width < minWidth) {[m
[32m+[m							[32mwidth  = minWidth;[m
[32m+[m							[32mheight = getScalar(width / ratio);[m
[32m+[m						[32m}[m
[32m+[m
[32m+[m						[32mif (width > maxWidth) {[m
[32m+[m							[32mwidth  = maxWidth;[m
[32m+[m							[32mheight = getScalar(width / ratio);[m
[32m+[m						[32m}[m
[32m+[m
[32m+[m						[32minner.width( width ).height( height );[m
[32m+[m
[32m+[m						[32mwrap.width( width + wPadding );[m
[32m+[m
[32m+[m						[32mwidth_  = wrap.width();[m
[32m+[m						[32mheight_ = wrap.height();[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m				[32m} else {[m
[32m+[m					[32mwidth  = Math.max(minWidth,  Math.min(width,  width  - (width_  - maxWidth_)));[m
[32m+[m					[32mheight = Math.max(minHeight, Math.min(height, height - (height_ - maxHeight_)));[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (scrollOut && scrolling === 'auto' && height < origHeight && (width + wPadding + scrollOut) < maxWidth_) {[m
[32m+[m				[32mwidth += scrollOut;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32minner.width( width ).height( height );[m
[32m+[m
[32m+[m			[32mwrap.width( width + wPadding );[m
[32m+[m
[32m+[m			[32mwidth_  = wrap.width();[m
[32m+[m			[32mheight_ = wrap.height();[m
[32m+[m
[32m+[m			[32mcanShrink = (width_ > maxWidth_ || height_ > maxHeight_) && width > minWidth && height > minHeight;[m
[32m+[m			[32mcanExpand = current.aspectRatio ? (width < origMaxWidth && height < origMaxHeight && width < origWidth && height < origHeight) : ((width < origMaxWidth || height < origMaxHeight) && (width < origWidth || height < origHeight));[m
[32m+[m
[32m+[m			[32m$.extend(current, {[m
[32m+[m				[32mdim : {[m
[32m+[m					[32mwidth	: getValue( width_ ),[m
[32m+[m					[32mheight	: getValue( height_ )[m
[32m+[m				[32m},[m
[32m+[m				[32morigWidth  : origWidth,[m
[32m+[m				[32morigHeight : origHeight,[m
[32m+[m				[32mcanShrink  : canShrink,[m
[32m+[m				[32mcanExpand  : canExpand,[m
[32m+[m				[32mwPadding   : wPadding,[m
[32m+[m				[32mhPadding   : hPadding,[m
[32m+[m				[32mwrapSpace  : height_ - skin.outerHeight(true),[m
[32m+[m				[32mskinSpace  : skin.height() - height[m
[32m+[m			[32m});[m
[32m+[m
[32m+[m			[32mif (!iframe && current.autoHeight && height > minHeight && height < maxHeight && !canExpand) {[m
[32m+[m				[32minner.height('auto');[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m_getPosition: function (onlyAbsolute) {[m
[32m+[m			[32mvar current  = F.current,[m
[32m+[m				[32mviewport = F.getViewport(),[m
[32m+[m				[32mmargin   = current.margin,[m
[32m+[m				[32mwidth    = F.wrap.width()  + margin[1] + margin[3],[m
[32m+[m				[32mheight   = F.wrap.height() + margin[0] + margin[2],[m
[32m+[m				[32mrez      = {[m
[32m+[m					[32mposition: 'absolute',[m
[32m+[m					[32mtop  : margin[0],[m
[32m+[m					[32mleft : margin[3][m
[32m+[m				[32m};[m
[32m+[m
[32m+[m			[32mif (current.autoCenter && current.fixed && !onlyAbsolute && height <= viewport.h && width <= viewport.w) {[m
[32m+[m				[32mrez.position = 'fixed';[m
[32m+[m
[32m+[m			[32m} else if (!current.locked) {[m
[32m+[m				[32mrez.top  += viewport.y;[m
[32m+[m				[32mrez.left += viewport.x;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mrez.top  = getValue(Math.max(rez.top,  rez.top  + ((viewport.h - height) * current.topRatio)));[m
[32m+[m			[32mrez.left = getValue(Math.max(rez.left, rez.left + ((viewport.w - width)  * current.leftRatio)));[m
[32m+[m
[32m+[m			[32mreturn rez;[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m_afterZoomIn: function () {[m
[32m+[m			[32mvar current = F.current;[m
[32m+[m
[32m+[m			[32mif (!current) {[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mF.isOpen = F.isOpened = true;[m
[32m+[m
[32m+[m			[32mF.wrap.css('overflow', 'visible').addClass('fancybox-opened');[m
[32m+[m
[32m+[m			[32mF.update();[m
[32m+[m
[32m+[m			[32m// Assign a click event[m
[32m+[m			[32mif ( current.closeClick || (current.nextClick && F.group.length > 1) ) {[m
[32m+[m				[32mF.inner.css('cursor', 'pointer').bind('click.fb', function(e) {[m
[32m+[m					[32mif (!$(e.target).is('a') && !$(e.target).parent().is('a')) {[m
[32m+[m						[32me.preventDefault();[m
[32m+[m
[32m+[m						[32mF[ current.closeClick ? 'close' : 'next' ]();[m
[32m+[m					[32m}[m
[32m+[m				[32m});[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Create a close button[m
[32m+[m			[32mif (current.closeBtn) {[m
[32m+[m				[32m$(current.tpl.closeBtn).appendTo(F.skin).bind('click.fb', function(e) {[m
[32m+[m					[32me.preventDefault();[m
[32m+[m
[32m+[m					[32mF.close();[m
[32m+[m				[32m});[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Create navigation arrows[m
[32m+[m			[32mif (current.arrows && F.group.length > 1) {[m
[32m+[m				[32mif (current.loop || current.index > 0) {[m
[32m+[m					[32m$(current.tpl.prev).appendTo(F.outer).bind('click.fb', F.prev);[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mif (current.loop || current.index < F.group.length - 1) {[m
[32m+[m					[32m$(current.tpl.next).appendTo(F.outer).bind('click.fb', F.next);[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mF.trigger('afterShow');[m
[32m+[m
[32m+[m			[32m// Stop the slideshow if this is the last item[m
[32m+[m			[32mif (!current.loop && current.index === current.group.length - 1) {[m
[32m+[m				[32mF.play( false );[m
[32m+[m
[32m+[m			[32m} else if (F.opts.autoPlay && !F.player.isActive) {[m
[32m+[m				[32mF.opts.autoPlay = false;[m
[32m+[m
[32m+[m				[32mF.play();[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m_afterZoomOut: function ( obj ) {[m
[32m+[m			[32mobj = obj || F.current;[m
[32m+[m
[32m+[m			[32m$('.fancybox-wrap').trigger('onReset').remove();[m
[32m+[m
[32m+[m			[32m$.extend(F, {[m
[32m+[m				[32mgroup  : {},[m
[32m+[m				[32mopts   : {},[m
[32m+[m				[32mrouter : false,[m
[32m+[m				[32mcurrent   : null,[m
[32m+[m				[32misActive  : false,[m
[32m+[m				[32misOpened  : false,[m
[32m+[m				[32misOpen    : false,[m
[32m+[m				[32misClosing : false,[m
[32m+[m				[32mwrap   : null,[m
[32m+[m				[32mskin   : null,[m
[32m+[m				[32mouter  : null,[m
[32m+[m				[32minner  : null[m
[32m+[m			[32m});[m
[32m+[m
[32m+[m			[32mF.trigger('afterClose', obj);[m
[32m+[m		[32m}[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32m/*[m
[32m+[m	[32m *	Default transitions[m
[32m+[m	[32m */[m
[32m+[m
[32m+[m	[32mF.transitions = {[m
[32m+[m		[32mgetOrigPosition: function () {[m
[32m+[m			[32mvar current  = F.current,[m
[32m+[m				[32melement  = current.element,[m
[32m+[m				[32morig     = current.orig,[m
[32m+[m				[32mpos      = {},[m
[32m+[m				[32mwidth    = 50,[m
[32m+[m				[32mheight   = 50,[m
[32m+[m				[32mhPadding = current.hPadding,[m
[32m+[m				[32mwPadding = current.wPadding,[m
[32m+[m				[32mviewport = F.getViewport();[m
[32m+[m
[32m+[m			[32mif (!orig && current.isDom && element.is(':visible')) {[m
[32m+[m				[32morig = element.find('img:first');[m
[32m+[m
[32m+[m				[32mif (!orig.length) {[m
[32m+[m					[32morig = element;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (isQuery(orig)) {[m
[32m+[m				[32mpos = orig.offset();[m
[32m+[m
[32m+[m				[32mif (orig.is('img')) {[m
[32m+[m					[32mwidth  = orig.outerWidth();[m
[32m+[m					[32mheight = orig.outerHeight();[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m			[32m} else {[m
[32m+[m				[32mpos.top  = viewport.y + (viewport.h - height) * current.topRatio;[m
[32m+[m				[32mpos.left = viewport.x + (viewport.w - width)  * current.leftRatio;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (F.wrap.css('position') === 'fixed' || current.locked) {[m
[32m+[m				[32mpos.top  -= viewport.y;[m
[32m+[m				[32mpos.left -= viewport.x;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mpos = {[m
[32m+[m				[32mtop     : getValue(pos.top  - hPadding * current.topRatio),[m
[32m+[m				[32mleft    : getValue(pos.left - wPadding * current.leftRatio),[m
[32m+[m				[32mwidth   : getValue(width  + wPadding),[m
[32m+[m				[32mheight  : getValue(height + hPadding)[m
[32m+[m			[32m};[m
[32m+[m
[32m+[m			[32mreturn pos;[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32mstep: function (now, fx) {[m
[32m+[m			[32mvar ratio,[m
[32m+[m				[32mpadding,[m
[32m+[m				[32mvalue,[m
[32m+[m				[32mprop       = fx.prop,[m
[32m+[m				[32mcurrent    = F.current,[m
[32m+[m				[32mwrapSpace  = current.wrapSpace,[m
[32m+[m				[32mskinSpace  = current.skinSpace;[m
[32m+[m
[32m+[m			[32mif (prop === 'width' || prop === 'height') {[m
[32m+[m				[32mratio = fx.end === fx.start ? 1 : (now - fx.start) / (fx.end - fx.start);[m
[32m+[m
[32m+[m				[32mif (F.isClosing) {[m
[32m+[m					[32mratio = 1 - ratio;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mpadding = prop === 'width' ? current.wPadding : current.hPadding;[m
[32m+[m				[32mvalue   = now - padding;[m
[32m+[m
[32m+[m				[32mF.skin[ prop ](  getScalar( prop === 'width' ?  value : value - (wrapSpace * ratio) ) );[m
[32m+[m				[32mF.inner[ prop ]( getScalar( prop === 'width' ?  value : value - (wrapSpace * ratio) - (skinSpace * ratio) ) );[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32mzoomIn: function () {[m
[32m+[m			[32mvar current  = F.current,[m
[32m+[m				[32mstartPos = current.pos,[m
[32m+[m				[32meffect   = current.openEffect,[m
[32m+[m				[32melastic  = effect === 'elastic',[m
[32m+[m				[32mendPos   = $.extend({opacity : 1}, startPos);[m
[32m+[m
[32m+[m			[32m// Remove "position" property that breaks older IE[m
[32m+[m			[32mdelete endPos.position;[m
[32m+[m
[32m+[m			[32mif (elastic) {[m
[32m+[m				[32mstartPos = this.getOrigPosition();[m
[32m+[m
[32m+[m				[32mif (current.openOpacity) {[m
[32m+[m					[32mstartPos.opacity = 0.1;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m			[32m} else if (effect === 'fade') {[m
[32m+[m				[32mstartPos.opacity = 0.1;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mF.wrap.css(startPos).animate(endPos, {[m
[32m+[m				[32mduration : effect === 'none' ? 0 : current.openSpeed,[m
[32m+[m				[32measing   : current.openEasing,[m
[32m+[m				[32mstep     : elastic ? this.step : null,[m
[32m+[m				[32mcomplete : F._afterZoomIn[m
[32m+[m			[32m});[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32mzoomOut: function () {[m
[32m+[m			[32mvar current  = F.current,[m
[32m+[m				[32meffect   = current.closeEffect,[m
[32m+[m				[32melastic  = effect === 'elastic',[m
[32m+[m				[32mendPos   = {opacity : 0.1};[m
[32m+[m
[32m+[m			[32mif (elastic) {[m
[32m+[m				[32mendPos = this.getOrigPosition();[m
[32m+[m
[32m+[m				[32mif (current.closeOpacity) {[m
[32m+[m					[32mendPos.opacity = 0.1;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mF.wrap.animate(endPos, {[m
[32m+[m				[32mduration : effect === 'none' ? 0 : current.closeSpeed,[m
[32m+[m				[32measing   : current.closeEasing,[m
[32m+[m				[32mstep     : elastic ? this.step : null,[m
[32m+[m				[32mcomplete : F._afterZoomOut[m
[32m+[m			[32m});[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32mchangeIn: function () {[m
[32m+[m			[32mvar current   = F.current,[m
[32m+[m				[32meffect    = current.nextEffect,[m
[32m+[m				[32mstartPos  = current.pos,[m
[32m+[m				[32mendPos    = { opacity : 1 },[m
[32m+[m				[32mdirection = F.direction,[m
[32m+[m				[32mdistance  = 200,[m
[32m+[m				[32mfield;[m
[32m+[m
[32m+[m			[32mstartPos.opacity = 0.1;[m
[32m+[m
[32m+[m			[32mif (effect === 'elastic') {[m
[32m+[m				[32mfield = direction === 'down' || direction === 'up' ? 'top' : 'left';[m
[32m+[m
[32m+[m				[32mif (direction === 'down' || direction === 'right') {[m
[32m+[m					[32mstartPos[ field ] = getValue(getScalar(startPos[ field ]) - distance);[m
[32m+[m					[32mendPos[ field ]   = '+=' + distance + 'px';[m
[32m+[m
[32m+[m				[32m} else {[m
[32m+[m					[32mstartPos[ field ] = getValue(getScalar(startPos[ field ]) + distance);[m
[32m+[m					[32mendPos[ field ]   = '-=' + distance + 'px';[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Workaround for http://bugs.jquery.com/ticket/12273[m
[32m+[m			[32mif (effect === 'none') {[m
[32m+[m				[32mF._afterZoomIn();[m
[32m+[m
[32m+[m			[32m} else {[m
[32m+[m				[32mF.wrap.css(startPos).animate(endPos, {[m
[32m+[m					[32mduration : current.nextSpeed,[m
[32m+[m					[32measing   : current.nextEasing,[m
[32m+[m					[32mcomplete : F._afterZoomIn[m
[32m+[m				[32m});[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32mchangeOut: function () {[m
[32m+[m			[32mvar previous  = F.previous,[m
[32m+[m				[32meffect    = previous.prevEffect,[m
[32m+[m				[32mendPos    = { opacity : 0.1 },[m
[32m+[m				[32mdirection = F.direction,[m
[32m+[m				[32mdistance  = 200;[m
[32m+[m
[32m+[m			[32mif (effect === 'elastic') {[m
[32m+[m				[32mendPos[ direction === 'down' || direction === 'up' ? 'top' : 'left' ] = ( direction === 'up' || direction === 'left' ? '-' : '+' ) + '=' + distance + 'px';[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mprevious.wrap.animate(endPos, {[m
[32m+[m				[32mduration : effect === 'none' ? 0 : previous.prevSpeed,[m
[32m+[m				[32measing   : previous.prevEasing,[m
[32m+[m				[32mcomplete : function () {[m
[32m+[m					[32m$(this).trigger('onReset').remove();[m
[32m+[m				[32m}[m
[32m+[m			[32m});[m
[32m+[m		[32m}[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m	[32m/*[m
[32m+[m	[32m *	Overlay helper[m
[32m+[m	[32m */[m
[32m+[m
[32m+[m	[32mF.helpers.overlay = {[m
[32m+[m		[32mdefaults : {[m
[32m+[m			[32mcloseClick : true,  // if true, fancyBox will be closed when user clicks on the overlay[m
[32m+[m			[32mspeedOut   : 200,   // duration of fadeOut animation[m
[32m+[m			[32mshowEarly  : true,  // indicates if should be opened immediately or wait until the content is ready[m
[32m+[m			[32mcss        : {},    // custom CSS properties[m
[32m+[m			[32mlocked     : !isTouch,  // if true, the content will be locked into overlay[m
[32m+[m			[32mfixed      : true   // if false, the overlay CSS position property will not be set to "fixed"[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32moverlay : null,   // current handle[m
[32m+[m		[32mfixed   : false,  // indicates if the overlay has position "fixed"[m
[32m+[m
[32m+[m		[32m// Public methods[m
[32m+[m		[32mcreate : function(opts) {[m
[32m+[m			[32mopts = $.extend({}, this.defaults, opts);[m
[32m+[m
[32m+[m			[32mif (this.overlay) {[m
[32m+[m				[32mthis.close();[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mthis.overlay = $('<div class="fancybox-overlay"></div>').appendTo( 'body' );[m
[32m+[m			[32mthis.fixed   = false;[m
[32m+[m
[32m+[m			[32mif (opts.fixed && F.defaults.fixed) {[m
[32m+[m				[32mthis.overlay.addClass('fancybox-overlay-fixed');[m
[32m+[m
[32m+[m				[32mthis.fixed = true;[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32mopen : function(opts) {[m
[32m+[m			[32mvar that = this;[m
[32m+[m
[32m+[m			[32mopts = $.extend({}, this.defaults, opts);[m
[32m+[m
[32m+[m			[32mif (this.overlay) {[m
[32m+[m				[32mthis.overlay.unbind('.overlay').width('auto').height('auto');[m
[32m+[m
[32m+[m			[32m} else {[m
[32m+[m				[32mthis.create(opts);[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (!this.fixed) {[m
[32m+[m				[32mW.bind('resize.overlay', $.proxy( this.update, this) );[m
[32m+[m
[32m+[m				[32mthis.update();[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (opts.closeClick) {[m
[32m+[m				[32mthis.overlay.bind('click.overlay', function(e) {[m
[32m+[m					[32mif ($(e.target).hasClass('fancybox-overlay')) {[m
[32m+[m						[32mif (F.isActive) {[m
[32m+[m							[32mF.close();[m
[32m+[m						[32m} else {[m
[32m+[m							[32mthat.close();[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m				[32m});[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mthis.overlay.css( opts.css ).show();[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32mclose : function() {[m
[32m+[m			[32m$('.fancybox-overlay').remove();[m
[32m+[m
[32m+[m			[32mW.unbind('resize.overlay');[m
[32m+[m
[32m+[m			[32mthis.overlay = null;[m
[32m+[m
[32m+[m			[32mif (this.margin !== false) {[m
[32m+[m				[32m$('body').css('margin-right', this.margin);[m
[32m+[m
[32m+[m				[32mthis.margin = false;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (this.el) {[m
[32m+[m				[32mthis.el.removeClass('fancybox-lock');[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Private, callbacks[m
[32m+[m
[32m+[m		[32mupdate : function () {[m
[32m+[m			[32mvar width = '100%', offsetWidth;[m
[32m+[m
[32m+[m			[32m// Reset width/height so it will not mess[m
[32m+[m			[32mthis.overlay.width(width).height('100%');[m
[32m+[m
[32m+[m			[32m// jQuery does not return reliable result for IE[m
[32m+[m			[32mif (IE) {[m
[32m+[m				[32moffsetWidth = Math.max(document.documentElement.offsetWidth, document.body.offsetWidth);[m
[32m+[m
[32m+[m				[32mif (D.width() > offsetWidth) {[m
[32m+[m					[32mwidth = D.width();[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m			[32m} else if (D.width() > W.width()) {[m
[32m+[m				[32mwidth = D.width();[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mthis.overlay.width(width).height(D.height());[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// This is where we can manipulate DOM, because later it would cause iframes to reload[m
[32m+[m		[32monReady : function (opts, obj) {[m
[32m+[m			[32m$('.fancybox-overlay').stop(true, true);[m
[32m+[m
[32m+[m			[32mif (!this.overlay) {[m
[32m+[m				[32mthis.margin = D.height() > W.height() || $('body').css('overflow-y') === 'scroll' ? $('body').css('margin-right') : false;[m
[32m+[m				[32mthis.el     = document.all && !document.querySelector ? $('html') : $('body');[m
[32m+[m
[32m+[m				[32mthis.create(opts);[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (opts.locked && this.fixed) {[m
[32m+[m				[32mobj.locked = this.overlay.append( obj.wrap );[m
[32m+[m				[32mobj.fixed  = false;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (opts.showEarly === true) {[m
[32m+[m				[32mthis.beforeShow.apply(this, arguments);[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32mbeforeShow : function(opts, obj) {[m
[32m+[m			[32mif (obj.locked) {[m
[32m+[m				[32mthis.el.addClass('fancybox-lock');[m
[32m+[m
[32m+[m				[32mif (this.margin !== false) {[m
[32m+[m					[32m$('body').css('margin-right', getScalar( this.margin ) + obj.scrollbarWidth);[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mthis.open(opts);[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32monUpdate : function() {[m
[32m+[m			[32mif (!this.fixed) {[m
[32m+[m				[32mthis.update();[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32mafterClose: function (opts) {[m
[32m+[m			[32m// Remove overlay if exists and fancyBox is not opening[m
[32m+[m			[32m// (e.g., it is not being open using afterClose callback)[m
[32m+[m			[32mif (this.overlay && !F.isActive) {[m
[32m+[m				[32mthis.overlay.fadeOut(opts.speedOut, $.proxy( this.close, this ));[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m	[32m/*[m
[32m+[m	[32m *	Title helper[m
[32m+[m	[32m */[m
[32m+[m
[32m+[m	[32mF.helpers.title = {[m
[32m+[m		[32mdefaults : {[m
[32m+[m			[32mtype     : 'float', // 'float', 'inside', 'outside' or 'over',[m
[32m+[m			[32mposition : 'bottom' // 'top' or 'bottom'[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32mbeforeShow: function (opts) {[m
[32m+[m			[32mvar current = F.current,[m
[32m+[m				[32mtext    = current.title,[m
[32m+[m				[32mtype    = opts.type,[m
[32m+[m				[32mtitle,[m
[32m+[m				[32mtarget;[m
[32m+[m
[32m+[m			[32mif ($.isFunction(text)) {[m
[32m+[m				[32mtext = text.call(current.element, current);[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (!isString(text) || $.trim(text) === '') {[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mtitle = $('<div class="fancybox-title fancybox-title-' + type + '-wrap">' + text + '</div>');[m
[32m+[m
[32m+[m			[32mswitch (type) {[m
[32m+[m				[32mcase 'inside':[m
[32m+[m					[32mtarget = F.skin;[m
[32m+[m				[32mbreak;[m
[32m+[m
[32m+[m				[32mcase 'outside':[m
[32m+[m					[32mtarget = F.wrap;[m
[32m+[m				[32mbreak;[m
[32m+[m
[32m+[m				[32mcase 'over':[m
[32m+[m					[32mtarget = F.inner;[m
[32m+[m				[32mbreak;[m
[32m+[m
[32m+[m				[32mdefault: // 'float'[m
[32m+[m					[32mtarget = F.skin;[m
[32m+[m
[32m+[m					[32mtitle.appendTo('body');[m
[32m+[m
[32m+[m					[32mif (IE) {[m
[32m+[m						[32mtitle.width( title.width() );[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32mtitle.wrapInner('<span class="child"></span>');[m
[32m+[m
[32m+[m					[32m//Increase bottom margin so this title will also fit into viewport[m
[32m+[m					[32mF.current.margin[2] += Math.abs( getScalar(title.css('margin-bottom')) );[m
[32m+[m				[32mbreak;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mtitle[ (opts.position === 'top' ? 'prependTo'  : 'appendTo') ](target);[m
[32m+[m		[32m}[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m	[32m// jQuery plugin initialization[m
[32m+[m	[32m$.fn.fancybox = function (options) {[m
[32m+[m		[32mvar index,[m
[32m+[m			[32mthat     = $(this),[m
[32m+[m			[32mselector = this.selector || '',[m
[32m+[m			[32mrun      = function(e) {[m
[32m+[m				[32mvar what = $(this).blur(), idx = index, relType, relVal;[m
[32m+[m
[32m+[m				[32mif (!(e.ctrlKey || e.altKey || e.shiftKey || e.metaKey) && !what.is('.fancybox-wrap')) {[m
[32m+[m					[32mrelType = options.groupAttr || 'data-fancybox-group';[m
[32m+[m					[32mrelVal  = what.attr(relType);[m
[32m+[m
[32m+[m					[32mif (!relVal) {[m
[32m+[m						[32mrelType = 'rel';[m
[32m+[m						[32mrelVal  = what.get(0)[ relType ];[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32mif (relVal && relVal !== '' && relVal !== 'nofollow') {[m
[32m+[m						[32mwhat = selector.length ? $(selector) : that;[m
[32m+[m						[32mwhat = what.filter('[' + relType + '="' + relVal + '"]');[m
[32m+[m						[32midx  = what.index(this);[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32moptions.index = idx;[m
[32m+[m
[32m+[m					[32m// Stop an event from bubbling if everything is fine[m
[32m+[m					[32mif (F.open(what, options) !== false) {[m
[32m+[m						[32me.preventDefault();[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m};[m
[32m+[m
[32m+[m		[32moptions = options || {};[m
[32m+[m		[32mindex   = options.index || 0;[m
[32m+[m
[32m+[m		[32mif (!selector || options.live === false) {[m
[32m+[m			[32mthat.unbind('click.fb-start').bind('click.fb-start', run);[m
[32m+[m
[32m+[m		[32m} else {[m
[32m+[m			[32mD.undelegate(selector, 'click.fb-start').delegate(selector + ":not('.fancybox-item, .fancybox-nav')", 'click.fb-start', run);[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mthis.filter('[data-fancybox-start=1]').trigger('click');[m
[32m+[m
[32m+[m		[32mreturn this;[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m	[32m// Tests that need a body at doc ready[m
[32m+[m	[32mD.ready(function() {[m
[32m+[m		[32mif ( $.scrollbarWidth === undefined ) {[m
[32m+[m			[32m// http://benalman.com/projects/jquery-misc-plugins/#scrollbarwidth[m
[32m+[m			[32m$.scrollbarWidth = function() {[m
[32m+[m				[32mvar parent = $('<div style="width:50px;height:50px;overflow:auto"><div/></div>').appendTo('body'),[m
[32m+[m					[32mchild  = parent.children(),[m
[32m+[m					[32mwidth  = child.innerWidth() - child.height( 99 ).innerWidth();[m
[32m+[m
[32m+[m				[32mparent.remove();[m
[32m+[m
[32m+[m				[32mreturn width;[m
[32m+[m			[32m};[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( $.support.fixedPosition === undefined ) {[m
[32m+[m			[32m$.support.fixedPosition = (function() {[m
[32m+[m				[32mvar elem  = $('<div style="position:fixed;top:20px;"></div>').appendTo('body'),[m
[32m+[m					[32mfixed = ( elem[0].offsetTop === 20 || elem[0].offsetTop === 15 );[m
[32m+[m
[32m+[m				[32melem.remove();[m
[32m+[m
[32m+[m				[32mreturn fixed;[m
[32m+[m			[32m}());[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m$.extend(F.defaults, {[m
[32m+[m			[32mscrollbarWidth : $.scrollbarWidth(),[m
[32m+[m			[32mfixed  : $.support.fixedPosition,[m
[32m+[m			[32mparent : $('body')[m
[32m+[m		[32m});[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m[32m}(window, document, jQuery));[m
