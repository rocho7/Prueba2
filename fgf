[32m+[m[32m      // Get the title[m
[32m+[m[32m      $entry = $xml->entry[$i];[m
[32m+[m[32m      $media = $entry->children('http://search.yahoo.com/mrss/');      $title = $media->group->title;[m
[32m+[m
[32m+[m[32m      // Get the duration in minutes and seconds, and then format it[m
[32m+[m[32m      $yt = $media->children('http://gdata.youtube.com/schemas/2007');      $attrs = $yt->duration->attributes();      $length_min = floor($attrs['seconds'] / 60);[m[41m[m
[32m+[m[32m      $length_sec = $attrs['seconds'] % 60;[m
[32m+[m[32m      $length_formatted = $length_min . (($length_min != 1) ? ' minutes, ':' minute, ') .[m
[32m+[m[32m        $length_sec . (($length_sec != 1) ? ' seconds':' second');[m
[32m+[m
[32m+[m[32m      // Get the video URL[m
[32m+[m[32m      $attrs = $media->group->player->attributes();      $video_url = $attrs['url'];[m
[32m+[m
[32m+[m[32m      // Get the thumbnail image URL[m
[32m+[m[32m      $attrs = $media->group->thumbnail[0]->attributes();      $thumbnail_url = $attrs['url'];[m[41m [m
[32m+[m
[32m+[m[32m      // Display the results for this entry[m
[32m+[m[32m      echo '<td style="vertical-align:bottom; text-align:center" width="' . (100 / NUM_VIDEOS) . '%"><a href="' . $video_url . '">' .[m
[32m+[m[32m        $title . '<br /><span style="font-size:smaller">' . $length_formatted . '</span><br /><img src="' . $thumbnail_url . '" /></a></td>';    }[m
[32m+[m[32m    echo '</tr></table>';[m
[32m+[m[32m  }[m
[32m+[m[32m  else {[m
