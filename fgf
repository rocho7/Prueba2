[32m+[m		[32m};[m
[32m+[m		[32mthis.displayLeadership = function ($leadership_row) {[m
[32m+[m			[32m//console.log($leadership_row);[m
[32m+[m			[32m$leadership_row.find('.leadership-item').each(function (index) {[m
[32m+[m				[32mvar speed = 225;[m
[32m+[m				[32mvar waitTimeGoingLeft = 400 - index * 100;[m
[32m+[m				[32mvar waitTimeGoingRight = index * 100;[m
[32m+[m				[32mvar _this = $(this);[m
[32m+[m				[32msetTimeout(function() {[m
[32m+[m					[32m_this.animate({[m
[32m+[m						[32mopacity: 1[m
[32m+[m					[32m}, speed);[m
[32m+[m				[32m}, waitTimeGoingRight);[m
[32m+[m			[32m});[m
[32m+[m		[32m};[m
[32m+[m		[32mthis.buildArrowTween = function(dir) {[m
[32m+[m			[32mvar returnTween;[m
[32m+[m			[32mswitch (dir) {[m
[32m+[m				[32mcase 'fadein':[m
[32m+[m					[32mreturnTween = [];[m
[32m+[m					[32mvar leftVal = 0,[m
[32m+[m						[32mtopVal = 0,[m
[32m+[m						[32mcenter = {x: 250, y: -3},[m
[32m+[m						[32mrad = 450,[m
[32m+[m						[32mvals = [],[m
[32m+[m						[32m// For circle: divide half-circle evenly by # of arrows[m
[32m+[m						[32mstartAng = 0, // Math.PI / 4;[m
[32m+[m						[32mangleSpan = 4 * Math.PI, // ath.PI, //2, //3,[m
[32m+[m						[32mnumberOfArrows = $arrows.length,[m
[32m+[m						[32mangleInc = Math.PI / 6, //angleSpan / numberOfArrows,[m
[32m+[m						[32mcurrentAng = startAng,[m
[32m+[m[32m            // For vertical mvnt[m
[32m+[m[32m            variationStart = 70,[m
[32m+[m[32m            variationAmt = 100[m
[32m+[m[32m            ;[m
[32m+[m[41m					[m
[32m+[m					[32m$arrows.each(function (ind, el) {[m
[32m+[m						[32mvals.push({[m
[32m+[m							[32m//x: center.x + Math.floor(rad * Math.cos(currentAng)),[m
[32m+[m							[32m//y: center.y - Math.floor(rad * Math.sin(currentAng))[m
[32m+[m[32m              y: variationStart[m[41m [m
[32m+[m[32m                + (variationAmt * Math.cos(currentAng))[m
[32m+[m[32m                + (ind * 5)[m
[32m+[m						[32m});[m
[32m+[m						[32mcurrentAng += angleInc;[m
[32m+[m					[32m});[m
[32m+[m					[32m$arrows.each(function (ind, el) {[m
[32m+[m						[32mvar valIndex = (numberOfArrows  - 1) - ind;[m
[32m+[m						[32mreturnTween.push ([m
[32m+[m[32m             TweenMax.fromTo([m
[32m+[m[32m               $(this),[m[41m [m
[32m+[m[32m               0.1,[m[41m [m
[32m+[m[32m               {css: {opacity:1,/*0,left: vals[valIndex].x,*/top: vals[valIndex].y}},[m
[32m+[m[32m               {css: {opacity:1,/*left: vals[valIndex].x,*/top: vals[valIndex].y}}[m
[32m+[m[32m               )[m
[32m+[m[32m             );[m
[32m+[m					[32m});[m
[32m+[m					[32mbreak;[m
[32m+[m				[32mcase 'position':[m
[32m+[m[32m          returnTween = [];[m
[32m+[m[32m          var vals = [],[m
[32m+[m[32m              pyramidLevel = 5,[m
[32m+[m[32m              currLevel = 0,[m
[32m+[m[32m              currSpot = 0,[m
[32m+[m[32m              startX = 188,[m
[32m+[m[32m              startY = 6,[m
[32m+[m[32m              incX = 33,[m
[32m+[m[32m              incY = 17;[m
[32m+[m[32m          $arrows.each(function (ind, el) {[m
[32m+[m[32m            vals.push({[m
[32m+[m[32m              x: startX + (incX * currSpot),[m
[32m+[m[32m              y: startY[m
[32m+[m[32m            });[m
[32m+[m[32m            currSpot += 1;[m
[32m+[m[32m            if (currSpot >= pyramidLevel) {[m
[32m+[m[32m              currSpot = 0;[m
[32m+[m[32m              pyramidLevel -= 1;[m
[32m+[m[32m              startX += (incX / 2);[m
[32m+[m[32m              startY += incY;[m
[32m+[m[32m            }[m
[32m+[m[32m          });[m
[32m+[m[32m          $arrows.each(function (ind, el) {[m
[32m+[m[32m            returnTween.push([m
[32m+[m[32m              TweenMax.to([m
[32m+[m[32m                $(this),[m
[32m+[m[32m                0.1,[m
[32m+[m[32m                {css: {left: vals[ind].x, top: vals[ind].y}}[m
[32m+[m[32m              )[m
[32m+[m[32m            );[m
[32m+[m[32m          });[m
[32m+[m[32m          break;[m
[32m+[m[32m        case 'out':[m
[32m+[m					[32mreturnTween = TweenMax.to($('.arrow'), 0.2, {css: {top: 400, opacity: 0}});[m
[32m+[m[32m          break;[m
[32m+[m			[32m}[m
[32m+[m			[32mreturn returnTween;[m
[32m+[m		[32m};[m
[32m+[m	[32m};[m
[32m+[m[32m})(jQuery);[m
