[32m+[m
[32m+[m
[32m+[m			[32m// Scroll tween animations[m
[32m+[m			[32m// Sweet spot tween[m
[32m+[m			[32mvar doNothingParam = 0.4;[m
[32m+[m			[32mvar animDuration = 0.4;[m
[32m+[m
[32m+[m			[32m$.superscrollorama({blocks: '.section'})[m
[32m+[m				[32m.pin($('#sweet-spot-pin'), 1800, {[m
[32m+[m					[32manim: (new TimelineLite())[m
[32m+[m					[32m.append( TweenMax.to($('.do-nothing'), doNothingParam, {}))[m
[32m+[m					[32m.append([m
[32m+[m						[32mTweenMax.to($('#sweet-spot-slide-1'), animDuration,[m
[32m+[m							[32m{css: {width: 0, height: 0, opacity: 0}}[m
[32m+[m						[32m)[m
[32m+[m					[32m)[m
[32m+[m					[32m.append( TweenMax.to($('.do-nothing'), doNothingParam, {}))[m
[32m+[m					[32m.append([m
[32m+[m						[32mTweenMax.to($('#sweet-spot-slide-2'), animDuration,[m
[32m+[m							[32m{css: {width: 0, height: 0, opacity: 0}}[m
