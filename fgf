[32m+[m[32m/*[m
[32m+[m[32mREQUIRED STYLES (be careful overriding)[m
[32m+[m[32m================================================================================ */[m
[32m+[m[32m/* When loading the player, the video tag is replaced with a DIV,[m
[32m+[m[32m   that will hold the video tag or object tag for other playback methods.[m
[32m+[m[32m   The div contains the video playback element (Flash or HTML5) and controls, and sets the width and height of the video.[m
[32m+[m
[32m+[m[32m   ** If you want to add some kind of border/padding (e.g. a frame), or special positioning, use another containing element.[m
[32m+[m[32m   Otherwise you risk messing up control positioning and full window mode. **[m
[32m+[m[32m*/[m
[32m+[m[32m.video-js {[m
[32m+[m[32m  background-color: #000; position: relative; padding: 0;[m
[32m+[m
[32m+[m[32m  /* Start with 10px for base font size so other dimensions can be em based and easily calculable. */[m
[32m+[m[32m  font-size: 10px;[m
[32m+[m
[32m+[m[32m  /* Allow poster to be vertially aligned. */[m
[32m+[m[32m  vertical-align: middle;[m
[32m+[m[32m  /*  display: table-cell; */ /*This works in Safari but not Firefox.*/[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* Playback technology elements expand to the width/height of the containing div. <video> or <object> */[m
[32m+[m[32m.video-js .vjs-tech { position: absolute; top: 0; left: 0; width: 100%; height: 100%; }[m
[32m+[m
[32m+[m[32m/* Fix for Firefox 9 fullscreen (only if it is enabled). Not needed when checking fullScreenEnabled. */[m
[32m+[m[32m.video-js:-moz-full-screen { position: absolute; }[m
[32m+[m
[32m+[m[32m/* Fullscreen Styles */[m
[32m+[m[32mbody.vjs-full-window {[m
[32m+[m[32m  padding: 0; margin: 0;[m
[32m+[m[32m  height: 100%; overflow-y: auto; /* Fix for IE6 full-window. http://www.cssplay.co.uk/layouts/fixed.html */[m
[32m+[m[32m}[m
[32m+[m[32m.video-js.vjs-fullscreen {[m
[32m+[m[32m  position: fixed; overflow: hidden; z-index: 1000; left: 0; top: 0; bottom: 0; right: 0; width: 100% !important; height: 100% !important;[m
[32m+[m[32m  _position: absolute; /* IE6 Full-window (underscore hack) */[m
[32m+[m[32m}[m
[32m+[m[32m.video-js:-webkit-full-screen {[m
[32m+[m[32m  width: 100% !important; height: 100% !important;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* Poster Styles */[m
