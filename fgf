[32m+[m					[32m{[m
[32m+[m						[32mvar delay = Math.random() * .25; // .5;[m
[32m+[m						[32m// TweenMax.fromTo($(this), 2, {css:{autoAlpha:0, top:$(this).data('y')+400}}, {css:{autoAlpha:1, top:$(this).data('y')}, ease:Expo.easeOut, delay:delay});[m
[32m+[m						[32mTweenMax.to($(this), 1.5, {css:{autoAlpha:1, top:$(this).data('y')}, ease:Expo.easeOut, delay:delay});[m
[32m+[m						[32m$(this).data('in', true);[m
[32m+[m					[32m}[m
[32m+[m					[32m/*else if( $(this).data('y') < scrollT + 200 && $(this).data('in') )[m
[32m+[m					[32m{[m
[32m+[m						[32mvar delay = Math.random()*.5;[m
[32m+[m						[32mTweenMax.to($(this), 1.5, {css:{autoAlpha:0, top:$(this).data('y')-300}, ease:Expo.easeInOut, delay:delay});[m
[32m+[m						[32m$(this).data('in', false);[m
[32m+[m					[32m}[m
[32m+[m					[32melse if( $(this).data('y') > scrollT + (_this.display.h + 100) && $(this).data('in') )[m
[32m+[m					[32m{[m
[32m+[m						[32mvar delay = Math.random()*.5;[m
[32m+[m						[32mTweenMax.to($(this), 1.5, {css:{autoAlpha:0, top:$(this).data('y')+300}, ease:Expo.easeInOut, delay:delay});[m
[32m+[m						[32m$(this).data('in', false);[m
[32m+[m					[32m}*/[m
[32m+[m				[32m})[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32mthis.getLeft = function(startPos, maxWidth ){[m
[32m+[m			[32mvar maxPos = maxWidth - 225;[m
[32m+[m			[32mvar newPos = startPos + (Math.random()*100);[m
[32m+[m			[32mif(newPos > maxPos){[m
[32m+[m				[32mnewPos = _this.getLeft(startPos, maxWidth);[m
[32m+[m				[32mconsole.log(newPos)[m
[32m+[m			[32m}[m[41m [m
[32m+[m			[32mreturn newPos;[m
[32m+[m		[32m}[m
[32m+[m		[32mthis.displayCapabilities = function () {}[m
[32m+[m	[32m}[m
[32m+[m[32m})(jQuery);[m
[32m+[m
[32m+[m[32m// main entry point aka on document ready[m
[32m+[m[32m!function($) {[m
[32m+[m	[32m$(function() {[m
[32m+[m		[32mif ($('body').hasClass('page-template-page-work-php')) {[m
[32m+[m			[32mvar workPage = new WorkPage($);[m
[32m+[m			[32mworkPage.init();[m
[32m+[m			[32m$(window).resize(workPage.updateDisplay);[m
[32m+[m			[32m$(window).scroll(workPage.onScroll);[m
[32m+[m
[32m+[m[41m			[m
[32m+[m			[32mvar initialized = false,[m
[32m+[m				[32mscroll_event_fired = false;[m
[32m+[m[41m			[m
[32m+[m			[32m$(window).on('scroll', function() {[m
[32m+[m				[32mscroll_event_fired = true;[m
[32m+[m			[32m});[m
[32m+[m
[32m+[m			[32m// Trigger this[m[41m [m
[32m+[m			[32msetInterval(function() {[m
[32m+[m				[32mif (scroll_event_fired && !initialized) {[m
[32m+[m					[32mworkPage.onScroll(null, true); // first parameter is event object, not used.  Second is parameter telling scroll to override scroll buffer just in this special initialization case.[m
[32m+[m					[32minitialized = true;[m
[32m+[m				[32m};[m
[32m+[m			[32m}, 300);[m
[32m+[m		[32m}[m
[32m+[m	[32m});[m
[32m+[m[32m}(jQuery);[m
