[1mnew file mode 100644[m
[1mindex 0000000..14767c7[m
[1m--- /dev/null[m
[1m+++ b/gitnn/Advertising/xylem.js[m
[36m@@ -0,0 +1,79 @@[m
[32m+[m[32m/**************************[m
[32m+[m[32m * @author Dan Alexander[m
[32m+[m[32m **************************/[m
[32m+[m
[32m+[m[32mxylem = {[m
[32m+[m	[32m// e is the element that we are going to be transitioning[m
[32m+[m	[32mTransitionManager : function(cs,e,sectionReachedCB,sectionChangedCB,d){[m
[32m+[m		[32mvar scrollTimer = 0;[m
[32m+[m		[32mvar scrollColumn = e;[m
[32m+[m		[32mvar contentSet = cs;[m
[32m+[m		[32mvar sectionReached = sectionReachedCB;[m
[32m+[m		[32mvar sectionChanged = sectionChangedCB;[m
[32m+[m		[32mvar oldTarget;[m
[32m+[m		[32mvar oldSectionTarget;[m
[32m+[m		[32mvar animatingToSection = false;[m
[32m+[m
[32m+[m		[32mvar tm = this;[m
[32m+[m
[32m+[m		[32mvar device = d;[m
[32m+[m
[32m+[m		[32mthis.initialize = function(){[m
[32m+[m			[32mscrollColumn = e;[m
[32m+[m			[32mif(!device) e.scroll(onWindowScroll);[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m		[32mthis.scrollToSection = function(s){[m
[32m+[m			[32mvar target = $(s);[m
[32m+[m			[32manimatingToSection = true;[m
[32m+[m
[32m+[m			[32m$('html,body').stop();[m
[32m+[m			[32m$('html,body').animate({scrollTop: Math.floor(target.offset().top)}, 500, onScrollAnimationFinished);[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m		[32mvar onScrollAnimationFinished = function(){[m
[32m+[m			[32manimatingToSection = false;[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m		[32mthis.onScrollFinished = function()[m
[32m+[m		[32m{[m
[32m+[m			[32mif(!device)[m
[32m+[m			[32m{[m
[32m+[m				[32m//find the closest element[m
[32m+[m				[32mvar target = getNearestSection();[m
[32m+[m
[32m+[m				[32msectionChanged.apply(target,[(oldSectionTarget === undefined || target.attr('id') != oldSectionTarget.attr('id'))]);[m
[32m+[m
[32m+[m				[32moldSectionTarget = target;[m
[32m+[m				[32m//let jqueryaddress hande the page transition from here[m
[32m+[m				[32m//$('html,body').stop();[m
[32m+[m				[32m//$('html,body').animate({scrollTop: target.offset().top}, 300);[m
[32m+[m			[32m}[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m		[32mvar onWindowScroll = function()[m
[32m+[m		[32m{[m
[32m+[m			[32mvar target = getNearestSection();[m
[32m+[m			[32mif(oldTarget === undefined || target.attr('id') != oldTarget.attr('id'))[m
[32m+[m			[32m{[m
[32m+[m				[32moldTarget = target;[m
[32m+[m				[32msectionReached.apply(target);[m
[32m+[m			[32m}[m
[32m+[m			[32mclearTimeout(scrollTimer);[m
[32m+[m			[32mif(!animatingToSection) scrollTimer = setTimeout( tm.onScrollFinished, 500 );[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m		[32mvar getNearestSection = function(){[m
[32m+[m			[32mvar targetOffset = 1000000000;[m
[32m+[m			[32mvar closestTarget;[m
[32m+[m			[32mcontentSet.each(function(){[m
[32m+[m				[32mif(Math.abs($(scrollColumn).scrollTop()-$(this).offset().top)<=targetOffset)[m
[32m+[m				[32m{[m
[32m+[m					[32mclosestTarget = $(this);[m
[32m+[m					[32mtargetOffset = Math.abs(scrollColumn.scrollTop()-$(this).offset().top);[m
[32m+[m				[32m}[m
[32m+[m			[32m});[m
[32m+[m			[32mreturn closestTarget;[m
[32m+[m		[32m};[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
\ No newline at end of file[m
