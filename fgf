[32m+[m	[32mfunction hover_nav_main_on ($nav_item, idx) {[m
[32m+[m		[32m$nav_item.css('left', -idx);[m
[32m+[m		[32mif (idx < nav_main_sprite_width) {[m
[32m+[m			[32msetTimeout(function() {[m
[32m+[m				[32mhover_nav_main_on($nav_item, idx + 100);[m
[32m+[m			[32m}, nav_main_hover_delay);[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m	[32mfunction hover_nav_main_off ($nav_item, idx) {[m
[32m+[m		[32m$nav_item.css('left', -idx);[m
[32m+[m		[32mif (idx > 0) {[m
[32m+[m			[32msetTimeout(function() {[m
[32m+[m				[32mhover_nav_main_off($nav_item, idx - 100);[m
[32m+[m			[32m}, nav_main_hover_delay);[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m	[32mfunction get_work_detail_slider_migration_distance($win) {[m
