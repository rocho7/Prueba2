[32m+[m[32m        function checkKey(evt) {[m
[32m+[m[32m            var evt = (evt) ? evt : ((event) ? event : null);[m
[32m+[m[32m            var node = (evt.target) ? evt.target : ((evt.srcElement) ? evt.srcElement : null);[m
[32m+[m[32m            if ((evt.keyCode == 13) && (node.type == "text") && (node.name == "txtFromAddr")) {[m
[32m+[m[32m                return false;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        document.onkeypress = checkKey;[m
