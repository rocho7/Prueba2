[32m+[m			[32mcloseBtn   : true,[m
[32m+[m			[32mcloseClick : false,[m
[32m+[m			[32mnextClick  : false,[m
[32m+[m			[32mmouseWheel : true,[m
[32m+[m			[32mautoPlay   : false,[m
[32m+[m			[32mplaySpeed  : 3000,[m
[32m+[m			[32mpreload    : 3,[m
[32m+[m			[32mmodal      : false,[m
