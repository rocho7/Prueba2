[32m+[m			[32mdisplay.h = window.innerHeight;[m
[32m+[m		[32m};[m
[32m+[m		[32mthis.resetSpacers = function () {[m
[32m+[m			[32m_this.updateDisplay();[m
[32m+[m			[32m// Sweet spot pin spacer[m
[32m+[m			[32m// add 102 for .headline[m
[32m+[m			[32mvar pinHeight = 102,[m
[32m+[m				[32mopenSpace,[m
[32m+[m				[32mnewSpacerHeight;[m
[32m+[m			[32m// add slider height[m
[32m+[m			[32mpinHeight = pinHeight + $('#sweet-spot-slide-1').height();[m
[32m+[m			[32mif (display.h < (pinHeight + 50 )) {[m
[32m+[m				[32m$('.section-sweet-spot .top-spacer').css('height', '150px');[m
[32m+[m				[32m$('.sweet-spot-spacer').css('height', '50px');[m
[32m+[m			[32m} else {[m
[32m+[m				[32mopenSpace = display.h - pinHeight;[m
[32m+[m				[32mnewSpacerHeight = Math.floor(openSpace / 2);[m
[32m+[m				[32m$('.section-sweet-spot .top-spacer').css('height', '0px');[m
[32m+[m				[32m$('.sweet-spot-spacer').css('height', newSpacerHeight + "px");[m
[32m+[m			[32m}[m
[32m+[m			[32m// Process pin spacer[m
[32m+[m			[32m// add 132 for .headline - 25 for padding[m
[32m+[m			[32mpinHeight = 107;[m
[32m+[m			[32m// add 60 for .process-body margin-top[m
[32m+[m			[32mpinHeight = pinHeight + 60;[m
[32m+[m			[32m// add .process-body height[m
[32m+[m			[32mpinHeight = pinHeight + $('.section-process .process-body').height();[m
[32m+[m			[32mif (display.h < (pinHeight + 50 )) {[m
[32m+[m				[32m$('.section-process .top-spacer').css('height', '150px');[m
[32m+[m				[32m$('.process-spacer').css('height', '50px');[m
[32m+[m			[32m} else {[m
[32m+[m				[32mopenSpace = display.h - pinHeight;[m
[32m+[m				[32mnewSpacerHeight = Math.floor(openSpace / 2);[m
[32m+[m				[32m// subtract 25 for .headline padding[m
[32m+[m				[32mnewSpacerHeight = newSpacerHeight - 25;[m
[32m+[m				[32m$('.section-process .top-spacer').css('height', '0px');[m
[32m+[m				[32m$('.process-spacer').css('height', newSpacerHeight + 'px');[m
[32m+[m			[32m}[m
[32m+[m		[32m};[m
[32m+[m		[32mthis.setInitialAnimationPositions = function () {[m
[32m+[m			[32mthis.resetSpacers();[m
[32m+[m[41m			[m
[32m+[m			[32m// Agency profile[m
