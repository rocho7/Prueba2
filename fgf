[32m+[m[32m * provided with the distribution.[m
[32m+[m[32m *[m[41m [m
[32m+[m[32m * Neither the name of the author nor the names of contributors may be used to endorse[m[41m [m
[32m+[m[32m * or promote products derived from this software without specific prior written permission.[m
[32m+[m[32m *[m[41m [m
[32m+[m[32m * THIS SOFTWARE IS PROVIDED BY THE COPYRIGHT HOLDERS AND CONTRIBUTORS "AS IS" AND ANY[m[41m [m
[32m+[m[32m * EXPRESS OR IMPLIED WARRANTIES, INCLUDING, BUT NOT LIMITED TO, THE IMPLIED WARRANTIES OF[m
[32m+[m[32m * MERCHANTABILITY AND FITNESS FOR A PARTICULAR PURPOSE ARE DISCLAIMED. IN NO EVENT SHALL THE[m
[32m+[m[32m *  COPYRIGHT OWNER OR CONTRIBUTORS BE LIABLE FOR ANY DIRECT, INDIRECT, INCIDENTAL, SPECIAL,[m
[32m+[m[32m *  EXEMPLARY, OR CONSEQUENTIAL DAMAGES (INCLUDING, BUT NOT LIMITED TO, PROCUREMENT OF SUBSTITUTE[m
[32m+[m[32m *  GOODS OR SERVICES; LOSS OF USE, DATA, OR PROFITS; OR BUSINESS INTERRUPTION) HOWEVER CAUSED[m[41m [m
[32m+[m[32m * AND ON ANY THEORY OF LIABILITY, WHETHER IN CONTRACT, STRICT LIABILITY, OR TORT (INCLUDING[m
[32m+[m[32m *  NEGLIGENCE OR OTHERWISE) ARISING IN ANY WAY OUT OF THE USE OF THIS SOFTWARE, EVEN IF ADVISED[m[41m [m
[32m+[m[32m * OF THE POSSIBILITY OF SUCH DAMAGE.[m[41m [m
[32m+[m[32m *[m
[32m+[m[32m*/[m
[32m+[m
[32m+[m[32m// t: current time, b: begInnIng value, c: change In value, d: duration[m
[32m+[m[32mjQuery.easing['jswing'] = jQuery.easing['swing'];[m
[32m+[m
[32m+[m[32mjQuery.extend( jQuery.easing,[m
[32m+[m[32m{[m
[32m+[m	[32mdef: 'easeOutQuad',[m
[32m+[m	[32mswing: function (x, t, b, c, d) {[m
[32m+[m		[32m//alert(jQuery.easing.default);[m
[32m+[m		[32mreturn jQuery.easing[jQuery.easing.def](x, t, b, c, d);[m
[32m+[m	[32m},[m
[32m+[m	[32measeInQuad: function (x, t, b, c, d) {[m
[32m+[m		[32mreturn c*(t/=d)*t + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeOutQuad: function (x, t, b, c, d) {[m
[32m+[m		[32mreturn -c *(t/=d)*(t-2) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeInOutQuad: function (x, t, b, c, d) {[m
[32m+[m		[32mif ((t/=d/2) < 1) return c/2*t*t + b;[m
[32m+[m		[32mreturn -c/2 * ((--t)*(t-2) - 1) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeInCubic: function (x, t, b, c, d) {[m
[32m+[m		[32mreturn c*(t/=d)*t*t + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeOutCubic: function (x, t, b, c, d) {[m
[32m+[m		[32mreturn c*((t=t/d-1)*t*t + 1) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeInOutCubic: function (x, t, b, c, d) {[m
[32m+[m		[32mif ((t/=d/2) < 1) return c/2*t*t*t + b;[m
[32m+[m		[32mreturn c/2*((t-=2)*t*t + 2) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeInQuart: function (x, t, b, c, d) {[m
[32m+[m		[32mreturn c*(t/=d)*t*t*t + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeOutQuart: function (x, t, b, c, d) {[m
[32m+[m		[32mreturn -c * ((t=t/d-1)*t*t*t - 1) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeInOutQuart: function (x, t, b, c, d) {[m
[32m+[m		[32mif ((t/=d/2) < 1) return c/2*t*t*t*t + b;[m
[32m+[m		[32mreturn -c/2 * ((t-=2)*t*t*t - 2) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeInQuint: function (x, t, b, c, d) {[m
[32m+[m		[32mreturn c*(t/=d)*t*t*t*t + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeOutQuint: function (x, t, b, c, d) {[m
[32m+[m		[32mreturn c*((t=t/d-1)*t*t*t*t + 1) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeInOutQuint: function (x, t, b, c, d) {[m
[32m+[m		[32mif ((t/=d/2) < 1) return c/2*t*t*t*t*t + b;[m
[32m+[m		[32mreturn c/2*((t-=2)*t*t*t*t + 2) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeInSine: function (x, t, b, c, d) {[m
[32m+[m		[32mreturn -c * Math.cos(t/d * (Math.PI/2)) + c + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeOutSine: function (x, t, b, c, d) {[m
[32m+[m		[32mreturn c * Math.sin(t/d * (Math.PI/2)) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeInOutSine: function (x, t, b, c, d) {[m
[32m+[m		[32mreturn -c/2 * (Math.cos(Math.PI*t/d) - 1) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeInExpo: function (x, t, b, c, d) {[m
[32m+[m		[32mreturn (t==0) ? b : c * Math.pow(2, 10 * (t/d - 1)) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeOutExpo: function (x, t, b, c, d) {[m
[32m+[m		[32mreturn (t==d) ? b+c : c * (-Math.pow(2, -10 * t/d) + 1) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeInOutExpo: function (x, t, b, c, d) {[m
[32m+[m		[32mif (t==0) return b;[m
[32m+[m		[32mif (t==d) return b+c;[m
[32m+[m		[32mif ((t/=d/2) < 1) return c/2 * Math.pow(2, 10 * (t - 1)) + b;[m
[32m+[m		[32mreturn c/2 * (-Math.pow(2, -10 * --t) + 2) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeInCirc: function (x, t, b, c, d) {[m
[32m+[m		[32mreturn -c * (Math.sqrt(1 - (t/=d)*t) - 1) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeOutCirc: function (x, t, b, c, d) {[m
[32m+[m		[32mreturn c * Math.sqrt(1 - (t=t/d-1)*t) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeInOutCirc: function (x, t, b, c, d) {[m
[32m+[m		[32mif ((t/=d/2) < 1) return -c/2 * (Math.sqrt(1 - t*t) - 1) + b;[m
[32m+[m		[32mreturn c/2 * (Math.sqrt(1 - (t-=2)*t) + 1) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeInElastic: function (x, t, b, c, d) {[m
[32m+[m		[32mvar s=1.70158;var p=0;var a=c;[m
[32m+[m		[32mif (t==0) return b;  if ((t/=d)==1) return b+c;  if (!p) p=d*.3;[m
[32m+[m		[32mif (a < Math.abs(c)) { a=c; var s=p/4; }[m
[32m+[m		[32melse var s = p/(2*Math.PI) * Math.asin (c/a);[m
[32m+[m		[32mreturn -(a*Math.pow(2,10*(t-=1)) * Math.sin( (t*d-s)*(2*Math.PI)/p )) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeOutElastic: function (x, t, b, c, d) {[m
[32m+[m		[32mvar s=1.70158;var p=0;var a=c;[m
[32m+[m		[32mif (t==0) return b;  if ((t/=d)==1) return b+c;  if (!p) p=d*.3;[m
[32m+[m		[32mif (a < Math.abs(c)) { a=c; var s=p/4; }[m
[32m+[m		[32melse var s = p/(2*Math.PI) * Math.asin (c/a);[m
[32m+[m		[32mreturn a*Math.pow(2,-10*t) * Math.sin( (t*d-s)*(2*Math.PI)/p ) + c + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeInOutElastic: function (x, t, b, c, d) {[m
[32m+[m		[32mvar s=1.70158;var p=0;var a=c;[m
[32m+[m		[32mif (t==0) return b;  if ((t/=d/2)==2) return b+c;  if (!p) p=d*(.3*1.5);[m
[32m+[m		[32mif (a < Math.abs(c)) { a=c; var s=p/4; }[m
[32m+[m		[32melse var s = p/(2*Math.PI) * Math.asin (c/a);[m
[32m+[m		[32mif (t < 1) return -.5*(a*Math.pow(2,10*(t-=1)) * Math.sin( (t*d-s)*(2*Math.PI)/p )) + b;[m
[32m+[m		[32mreturn a*Math.pow(2,-10*(t-=1)) * Math.sin( (t*d-s)*(2*Math.PI)/p )*.5 + c + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeInBack: function (x, t, b, c, d, s) {[m
[32m+[m		[32mif (s == undefined) s = 1.70158;[m
[32m+[m		[32mreturn c*(t/=d)*t*((s+1)*t - s) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeOutBack: function (x, t, b, c, d, s) {[m
[32m+[m		[32mif (s == undefined) s = 1.70158;[m
[32m+[m		[32mreturn c*((t=t/d-1)*t*((s+1)*t + s) + 1) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeInOutBack: function (x, t, b, c, d, s) {[m
[32m+[m		[32mif (s == undefined) s = 1.70158;[m[41m [m
[32m+[m		[32mif ((t/=d/2) < 1) return c/2*(t*t*(((s*=(1.525))+1)*t - s)) + b;[m
[32m+[m		[32mreturn c/2*((t-=2)*t*(((s*=(1.525))+1)*t + s) + 2) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeInBounce: function (x, t, b, c, d) {[m
[32m+[m		[32mreturn c - jQuery.easing.easeOutBounce (x, d-t, 0, c, d) + b;[m
[32m+[m	[32m},[m
[32m+[m	[32measeOutBounce: function (x, t, b, c, d) {[m
[32m+[m		[32mif ((t/=d) < (1/2.75)) {[m
[32m+[m			[32mreturn c*(7.5625*t*t) + b;[m
[32m+[m		[32m} else if (t < (2/2.75)) {[m
[32m+[m			[32mreturn c*(7.5625*(t-=(1.5/2.75))*t + .75) + b;[m
[32m+[m		[32m} else if (t < (2.5/2.75)) {[m
[32m+[m			[32mreturn c*(7.5625*(t-=(2.25/2.75))*t + .9375) + b;[m
[32m+[m		[32m} else {[m
[32m+[m			[32mreturn c*(7.5625*(t-=(2.625/2.75))*t + .984375) + b;[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m	[32measeInOutBounce: function (x, t, b, c, d) {[m
[32m+[m		[32mif (t < d/2) return jQuery.easing.easeInBounce (x, t*2, 0, c, d) * .5 + b;[m
[32m+[m		[32mreturn jQuery.easing.easeOutBounce (x, t*2-d, 0, c, d) * .5 + c*.5 + b;[m
[32m+[m	[32m}[m
[32m+[m[32m});[m
[32m+[m
