[32m+[m
[32m+[m			[32mautoResize  : true,[m
[32m+[m			[32mautoCenter  : !isTouch,[m
[32m+[m			[32mfitToView   : true,[m
[32m+[m			[32maspectRatio : false,[m
[32m+[m			[32mtopRatio    : 0.5,[m
[32m+[m			[32mleftRatio   : 0.5,[m
[32m+[m
[32m+[m			[32mscrolling : 'auto', // 'auto', 'yes' or 'no'[m
[32m+[m			[32mwrapCSS   : '',[m
