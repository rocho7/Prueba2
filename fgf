[32m+[m	[32m// animates the page to the previous one[m
[32m+[m	[32mfunction prevPage(){[m
[32m+[m		[32mif( isAnimating ) {[m
[32m+[m			[32mreturn false;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32manimatePage('left');[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction animatePage(direction){[m
[32m+[m		[32mif( isAnimating ) {[m
[32m+[m			[32mreturn false;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32misAnimating = true;[m
[32m+[m
[32m+[m		[32mvar xPos;[m
[32m+[m		[32mvar rotY;[m
[32m+[m
[32m+[m		[32mif(direction === 'left'){[m
[32m+[m			[32mxPos = ($main.position().left + slideWidth > 0 )? 0:$main.position().left + slideWidth;[m
[32m+[m
[32m+[m			[32mvar xDist = xPos-$main.position().left;[m
[32m+[m			[32mrotY = -(xDist/slideWidth)*rotationYAmount;[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32mxPos = (($main.position().left - slideWidth) < $(window).width()-$main.width() )? $(window).width()-$main.width():$main.position().left - slideWidth;[m
[32m+[m
[32m+[m			[32mvar xDist = $main.position().left-xPos;[m
[32m+[m			[32mrotY = (xDist/slideWidth)*rotationYAmount;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mTweenLite.to($main, 0.5, {'left':xPos, onComplete:onEndAnimation});[m
[32m+[m
[32m+[m		[32m$slides.each(function(index, el) {[m
[32m+[m			[32mvar tl = new TimelineLite();[m
[32m+[m			[32mtl.add( TweenMax.to(this, 0.5, {css:{rotationY:rotY}}), index*0.05);[m
[32m+[m			[32mtl.add( TweenMax.to(this, 1, {css:{rotationY:0}, ease:Elastic.easeOut}));[m
[32m+[m		[32m});[m
[32m+[m[41m		[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction onEndAnimation( $outpage, $inpage ) {[m
[32m+[m		[32mendCurrPage = false;[m
[32m+[m		[32mendNextPage = false;[m
[32m+[m		[32m//resetPage( $outpage, $inpage );[m
[32m+[m		[32misAnimating = false;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction resetPage( $outpage, $inpage ) {[m
[32m+[m		[32m$outpage.attr( 'class', $outpage.data( 'originalClassList' ) );[m
[32m+[m		[32m$inpage.attr( 'class', $inpage.data( 'originalClassList' ) + ' slide-current' );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m//init();[m
[32m+[m
[32m+[m	[32mreturn { init : init };[m
[32m+[m
[32m+[m[32m};[m
