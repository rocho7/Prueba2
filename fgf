[32m+[m
[32m+[m[32m.fancybox-nav span {[m
[32m+[m	[32mposition: absolute;[m
[32m+[m	[32mtop: 50%;[m
[32m+[m	[32mwidth: 36px;[m
[32m+[m	[32mheight: 34px;[m
